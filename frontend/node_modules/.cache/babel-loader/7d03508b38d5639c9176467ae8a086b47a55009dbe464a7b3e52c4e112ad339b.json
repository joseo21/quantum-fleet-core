,"sourcesContent":["<template>\n  <div>\n    <!-- Overlay móvil -->\n    <div v-if=\"isOpen\" class=\"fixed inset-0 bg-black bg-opacity-50 z-40 md:hidden\"\n      @click=\"$emit('update:isOpen', false)\"></div>\n\n    <aside :class=\"[\n      'fixed md:relative top-0 left-0 h-full bg-[#162d38] text-white border-r border-gray-700 flex flex-col transform transition-transform duration-300 z-50',\n      isOpen ? 'translate-x-0' : '-translate-x-full',\n      'md:translate-x-0 w-64'\n    ]\">\n      <!-- Header -->\n      <header class=\"p-4 flex justify-end items-center bg-gray-800 border-b \">\n        <!-- Hamburger solo en mobile -->\n        <button class=\"md:hidden text-white text-2xl\" @click=\"$emit('update:isOpen', false)\">\n          ✕\n        </button>\n      </header>\n\n      <!-- Menú -->\n      <nav class=\"flex-1 overflow-y-auto px-2 pb-4 space-y-1\">\n        <ul class=\"space-y-1\">\n          <li v-for=\"item in menuItems\" :key=\"item.text\">\n            <router-link v-if=\"item.route\" :to=\"item.route\"\n              class=\"flex items-center gap-x-3.5 py-2 px-2.5 rounded-lg text-sm hover:bg-[#ff6600]\">\n              {{ item.text }}\n            </router-link>\n            <a v-else :href=\"item.href\"\n              class=\"flex items-center gap-x-3.5 py-2 px-2.5 rounded-lg text-sm hover:bg-[#ff6600]\">\n              {{ item.text }}\n            </a>\n          </li>\n\n        </ul>\n      </nav>\n    </aside>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"AppSidebar\",\n  props: { isOpen: { type: Boolean, default: false } },\n  data() {\n    return {\n      profileImage:\n        localStorage.getItem(\"profileImage\") ||\n        \"https://img.freepik.com/vector-premium/perfil-hombre_1083548-15963.jpg\",\n      menuItems: [\n        { text: \"Dashboard\", route: \"/\" },\n        { text: \"Dispositivos\", route: \"/dispositivos\" },\n        { text: \"Alertas\", route: \"/alertas\" },\n        { text: \"Motor de Reglas\", route: \"/motor\" },\n        { text: \"Fuentes de Datos\", route: \"/fuentes\" },\n        { text: \"Usuarios\", route: \"/usuarios\" },\n        { text: \"Empresas\", route: \"/empresas\" },\n        { text: \"Reportes/Informes\", route: \"/reportes\" },\n        { text: \"Mantenciones\", route: \"/mantenciones\" },\n        { text: \"Configuración\", route: \"/configuracion\" }\n      ]\n    };\n  },\n  methods: {\n    selectFile() { this.$refs.fileInput.click(); },\n    previewImage(event) {\n      const file = event.target.files[0];\n      if (!file) return;\n      const reader = new FileReader();\n      reader.onload = () => {\n        this.profileImage = reader.result;\n        localStorage.setItem(\"profileImage\", reader.result);\n      };\n      reader.readAsDataURL(file);\n    },\n    logout() {\n      console.log(\"Cerrar sesión\");\n    }\n  }\n};\n</script>\n"],"mappings":"AAwCA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IAAEC,MAAM,EAAE;MAAEC,IAAI,EAAEC,OAAO;MAAEC,OAAO,EAAE;IAAM;EAAE,CAAC;EACpDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EACVC,YAAY,CAACC,OAAO,CAAC,cAAc,KACnC,wEAAwE;MAC1EC,SAAS,EAAE,CACT;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAI,CAAC,EACjC;QAAED,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE;MAAgB,CAAC,EAChD;QAAED,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE;MAAW,CAAC,EACtC;QAAED,IAAI,EAAE,iBAAiB;QAAEC,KAAK,EAAE;MAAS,CAAC,EAC5C;QAAED,IAAI,EAAE,kBAAkB;QAAEC,KAAK,EAAE;MAAW,CAAC,EAC/C;QAAED,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAY,CAAC,EACxC;QAAED,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAY,CAAC,EACxC;QAAED,IAAI,EAAE,mBAAmB;QAAEC,KAAK,EAAE;MAAY,CAAC,EACjD;QAAED,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE;MAAgB,CAAC,EAChD;QAAED,IAAI,EAAE,eAAe;QAAEC,KAAK,EAAE;MAAiB;IAErD,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MAAE,IAAI,CAACC,KAAK,CAACC,SAAS,CAACC,KAAK,CAAC,CAAC;IAAE,CAAC;IAC9CC,YAAYA,CAACC,KAAK,EAAE;MAClB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAClC,IAAI,CAACF,IAAI,EAAE;MACX,MAAMG,MAAK,GAAI,IAAIC,UAAU,CAAC,CAAC;MAC/BD,MAAM,CAACE,MAAK,GAAI,MAAM;QACpB,IAAI,CAAClB,YAAW,GA