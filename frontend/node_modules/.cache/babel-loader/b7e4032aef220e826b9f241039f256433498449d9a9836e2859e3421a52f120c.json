{"ast":null,"code":"import SvgIcon from '@/components/icons/SvgIcon.vue';\nexport default {\n  name: 'ModalAgregar',\n  components: {\n    SvgIcon\n  },\n  props: {\n    showAddModal: {\n      type: Boolean,\n      required: true\n    },\n    selectedPatente: {\n      type: String,\n      required: true\n    },\n    mantenciones: {\n      type: Array,\n      required: true\n    },\n    selectedMantencion: {\n      type: [Object, Number],\n      default: null\n    } // Nuevo\n  },\n  data() {\n    return {\n      step: 1,\n      templates: [{\n        id: 1,\n        name: 'Revisión Tecnica',\n        type: 'Por Tiempo',\n        description: 'Verificación completa del vehículo según normativa',\n        used: false\n      }, {\n        id: 2,\n        name: 'Permiso de Circulación',\n        type: 'Por Tiempo',\n        description: 'Gestión y pago del permiso de circulación anual',\n        used: false\n      }, {\n        id: 3,\n        name: 'Cambio de Neumáticos',\n        type: 'Por Odómetro',\n        description: 'Rotación, balanceo y revisión del desgaste de neumáticos',\n        used: false\n      }, {\n        id: 4,\n        name: 'Cambio de Aceite',\n        type: 'Por Odómetro',\n        description: 'Sustitución de aceite y filtro para mantener motor óptimo',\n        used: false\n      }, {\n        id: 5,\n        name: 'Cambio de Filtro de Aire',\n        type: 'Por Odómetro',\n        description: 'Reemplazo del filtro de aire para mejorar eficiencia del motor',\n        used: false\n      }],\n      selectedTemplate: null,\n      maintenance: {\n        name: '',\n        type: '',\n        date: '',\n        km: '',\n        details: '',\n        intervalo: '',\n        email: '',\n        alerta: '',\n        fechaUltima: ''\n      },\n      isModalButtonDisabled: false\n    };\n  },\n  methods: {\n    selectTemplate(template) {\n      if (template.used) return;\n      this.selectedTemplate = template;\n      this.maintenance.name = template.name;\n      this.maintenance.type = template.type;\n    },\n    goToNextStep() {\n      if (this.selectedTemplate) this.step = 2;\n    },\n    cancel() {\n      // Emitimos evento para que el padre cierre el modal\n      this.$emit('close');\n\n      // Reseteamos pasos y campos\n      this.step = 1;\n      this.selectedTemplate = null;\n      this.maintenance = {\n        name: '',\n        type: '',\n        date: '',\n        km: '',\n        details: '',\n        intervalo: '',\n        email: '',\n        alerta: '',\n        fechaUltima: ''\n      };\n    },\n    saveMaintenance() {\n      console.log('--- saveMaintenance triggered ---');\n      console.log('maintenance actual:', this.maintenance);\n      console.log('selectedTemplate:', this.selectedTemplate);\n      console.log('selectedMantencion:', this.selectedMantencion);\n      if (!this.maintenance) return;\n      const nuevaMantencion = {\n        nombre: this.maintenance.name,\n        tipo: this.maintenance.type,\n        intervalo: Number(this.maintenance.intervalo || 0),\n        ultimo: this.selectedTemplate?.type === 'Por Odómetro' ? Number(this.maintenance.km || 0) : this.maintenance.fechaUltima,\n        actual: this.selectedTemplate?.type === 'Por Odómetro' ? Number(this.maintenance.km || 0) : new Date().toISOString().split('T')[0],\n        alerta: Number(this.maintenance.alerta || 0),\n        email: this.maintenance.email || '',\n        fechaCreacion: new Date().toISOString()\n      };\n      console.log('nuevaMantencion a emitir:', nuevaMantencion);\n\n      // Emitimos al padre\n      this.$emit('save', nuevaMantencion);\n      if (!this.selectedMantencion) {\n        this.selectedTemplate.used = true;\n      }\n\n      // Cerramos modal usando el método cancel\n      this.cancel();\n      console.log('Modal cerrado');\n    },\n    onlyNumber(e) {\n      const char = String.fromCharCode(e.keyCode);\n      if (!/[0-9]/.test(char)) e.preventDefault();\n    }\n  },\n  computed: {\n    ultimoCampoLabel() {\n      if (!this.selectedTemplate) return '';\n      return this.selectedTemplate.type === 'Por Odómetro' ? 'Último Kilometraje' : 'Fecha Última Mantención';\n    },\n    ultimoCampoValue: {\n      get() {\n        if (this.selectedTemplate?.type === 'Por Odómetro') {\n          // Ya no toma el valor del odómetro actual\n          return this.maintenance.km;\n        } else {\n          return this.maintenance.fechaUltima;\n        }\n      },\n      set(value) {\n        if (this.selectedTemplate?.type === 'Por Odómetro') {\n          this.maintenance.km = value;\n        } else {\n          this.maintenance.fechaUltima = value;\n        }\n      }\n    }\n  },\n  watch: {\n    selectedMantencion: {\n      immediate: true,\n      handler(val) {\n        if (val !== null) {\n          // Si es número, estamos editando\n          if (typeof val === 'number') {\n            const m = this.mantenciones[val];\n            if (m) this.maintenance = {\n              ...m\n            };\n            this.step = 2;\n          } else {\n            this.step = 1;\n          }\n        } else {\n          this.maintenance = {\n            name: '',\n            type: '',\n            date: '',\n            km: '',\n            details: '',\n            intervalo: '',\n            email: '',\n            alerta: '',\n            fechaUltima: ''\n          };\n          this.step = 1;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["SvgIcon","name","components","props","showAddModal","type","Boolean","required","selectedPatente","String","mantenciones","Array","selectedMantencion","Object","Number","default","data","step","templates","id","description","used","selectedTemplate","maintenance","date","km","details","intervalo","email","alerta","fechaUltima","isModalButtonDisabled","methods","selectTemplate","template","goToNextStep","cancel","$emit","saveMaintenance","console","log","nuevaMantencion","nombre","tipo","ultimo","actual","Date","toISOString","split","fechaCreacion","onlyNumber","e","char","fromCharCode","keyCode","test","preventDefault","computed","ultimoCampoLabel","ultimoCampoValue","get","set","value","watch","immediate","handler","val","m"],"sources":["/home/sebastian/quantum-fleet-core/frontend/src/components/MantencionesUi/ModalAgregar.vue"],"sourcesContent":["<template>\n    <!-- Modal multi-paso -->\n    <div v-if=\"showAddModal\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-4\">\n        <div\n            class=\"bg-white dark:bg-gray-900 text-gray-900 dark:text-gray-100 rounded-lg relative flex flex-col w-full max-w-3xl h-auto p-6 border border-gray-300 dark:border-gray-700 shadow-xl\">\n            <!-- Botón volver paso 2 -> 1 -->\n            <button v-if=\"step === 2\" @click=\"step = 1\"\n                class=\"absolute top-4 left-4 p-2 rounded hover:bg-gray-200 dark:hover:bg-gray-700 transition\"\n                title=\"Volver\">\n                <SvgIcon name=\"arrow-left\" class=\"w-6 h-6\" />\n            </button>\n\n            <h2 class=\"text-2xl font-bold mb-4 text-center text-[#102372] dark:text-[#ff6600]\">\n                {{ step === 1 ? \"Seleccionar Template\" : `Agregar Mantención Vehículo (${selectedPatente})` }}\n            </h2>\n\n\n\n            <span class=\"h-px bg-gray-300 dark:bg-gray-700 mb-4\"></span>\n\n            <!-- Paso 1: Seleccionar Template -->\n            <div v-if=\"step === 1\" class=\"flex flex-col items-center gap-4 py-4\">\n                <!-- Fila superior -->\n                <div class=\"flex justify-center gap-4 flex-wrap\">\n                    <div v-for=\"template in templates\" :key=\"template.id\" @click=\"selectTemplate(template)\" :class=\"[\n                        'relative p-6 w-48 rounded-lg border cursor-pointer text-center transition-all',\n                        template.used\n                            ? 'border-gray-500 bg-gray-200 dark:bg-gray-800 opacity-60 cursor-not-allowed'\n                            : selectedTemplate?.id === template.id\n                                ? 'border-[#ff6600] bg-orange-50 dark:bg-[#ff6600]/20 dark:border-[#ff6600]'\n                                : 'border-gray-300 dark:border-gray-600 hover:bg-gray-100 dark:hover:bg-gray-800'\n                    ]\">\n                        <h3 class=\"text-lg font-semibold mb-2\">{{ template.name }}</h3>\n                        <p class=\"text-gray-600 dark:text-gray-300 text-sm mb-2\">\n                            {{ template.description }}\n                        </p>\n\n                        <!-- Etiqueta de bloqueo -->\n                        <div v-if=\"template.used\"\n                            class=\"absolute inset-0 flex items-center justify-center bg-black bg-opacity-50 text-white rounded-lg text-sm font-semibold\">\n                            Este vehículo ya tiene este tipo de mantención\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <form v-else @submit.prevent=\"saveMaintenance\" class=\"grid grid-cols-2 gap-4 py-2 overflow-y-auto\">\n                <div>\n                    <label class=\"block text-sm mb-1\">Nombre de la Mantención</label>\n                    <input type=\"text\"\n                        class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n                        v-model=\"maintenance.name\" disabled />\n                </div>\n\n                <div>\n                    <label class=\"block text-sm mb-1\">Tipo de Mantención</label>\n                    <input type=\"text\"\n                        class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n                        v-model=\"maintenance.type\" disabled />\n                </div>\n\n                <div>\n                    <label class=\"block text-sm mb-1\">Intervalo</label>\n                    <input type=\"number\" placeholder=\"Ej: 30 días o 5000 km\"\n                        class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n                        v-model=\"maintenance.intervalo\" @keypress=\"onlyNumber($event)\" />\n\n                </div>\n\n                <div>\n                    <label class=\"block text-sm mb-1\">Email de contacto</label>\n                    <input type=\"email\" placeholder=\"Ej: contacto@empresa.cl\"\n                        class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n                        v-model=\"maintenance.email\" />\n                </div>\n\n                <div>\n                    <label class=\"block text-sm mb-1\">Alerta</label>\n                    <input type=\"number\" placeholder=\"Ej: 5 días antes o 200 km antes\"\n                        class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n                        v-model=\"maintenance.alerta\" @keypress=\"onlyNumber($event)\" />\n\n                </div>\n\n                <div>\n                    <label class=\"block text-sm mb-1\">{{ ultimoCampoLabel }}</label>\n                    <input v-model=\"ultimoCampoValue\"\n                        :type=\"selectedTemplate?.type === 'Por Odómetro' ? 'number' : 'date'\" placeholder=\"Ej: 45000\"\n                        class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n                        @keypress=\"selectedTemplate?.type === 'Por Odómetro' ? onlyNumber($event) : null\" />\n                </div>\n\n            </form>\n\n\n            <!-- Botones del modal -->\n            <div class=\"flex justify-between mt-6 gap-2\">\n                <button @click=\"cancel\"\n                    class=\"bg-gray-300 dark:bg-gray-700 hover:bg-gray-400 dark:hover:bg-gray-600 text-gray-900 dark:text-white px-4 py-2 rounded w-[48%] transition\">\n                    Cancelar\n                </button>\n\n                <button v-if=\"step === 1\" @click=\"goToNextStep\" :disabled=\"!selectedTemplate\" :class=\"[\n                    'px-4 py-2 rounded w-[48%] font-semibold transition',\n                    selectedTemplate\n                        ? 'bg-[#102372] hover:bg-[#0d1c5a] text-white'\n                        : 'bg-[#102372] opacity-50 cursor-not-allowed text-white'\n                ]\">\n                    Siguiente\n                </button>\n\n                <button v-else type=\"button\" @click=\"saveMaintenance\" :disabled=\"isModalButtonDisabled\" :class=\"[\n                    'px-4 py-2 rounded w-[48%] font-semibold transition',\n                    isModalButtonDisabled\n                        ? 'bg-[#102372] opacity-50 cursor-not-allowed text-white'\n                        : 'bg-[#102372] hover:bg-[#0d1c5a] text-white'\n                ]\">\n                    Guardar\n                </button>\n\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport SvgIcon from '@/components/icons/SvgIcon.vue'\n\nexport default {\n    name: 'ModalAgregar',\n    components: { SvgIcon },\n    props: {\n        showAddModal: { type: Boolean, required: true },\n        selectedPatente: { type: String, required: true },\n        mantenciones: { type: Array, required: true },\n        selectedMantencion: { type: [Object, Number], default: null } // Nuevo\n    },\n    data() {\n        return {\n            step: 1,\n            templates: [\n                { id: 1, name: 'Revisión Tecnica', type: 'Por Tiempo', description: 'Verificación completa del vehículo según normativa', used: false },\n                { id: 2, name: 'Permiso de Circulación', type: 'Por Tiempo', description: 'Gestión y pago del permiso de circulación anual', used: false },\n                { id: 3, name: 'Cambio de Neumáticos', type: 'Por Odómetro', description: 'Rotación, balanceo y revisión del desgaste de neumáticos', used: false },\n                { id: 4, name: 'Cambio de Aceite', type: 'Por Odómetro', description: 'Sustitución de aceite y filtro para mantener motor óptimo', used: false },\n                { id: 5, name: 'Cambio de Filtro de Aire', type: 'Por Odómetro', description: 'Reemplazo del filtro de aire para mejorar eficiencia del motor', used: false },\n            ],\n            selectedTemplate: null,\n            maintenance: {\n                name: '',\n                type: '',\n                date: '',\n                km: '',\n                details: '',\n                intervalo: '',\n                email: '',\n                alerta: '',\n                fechaUltima: ''\n            },\n\n            isModalButtonDisabled: false,\n        }\n    },\n    methods: {\n        selectTemplate(template) {\n            if (template.used) return\n            this.selectedTemplate = template\n            this.maintenance.name = template.name\n            this.maintenance.type = template.type\n        },\n        goToNextStep() {\n            if (this.selectedTemplate) this.step = 2\n        },\n        cancel() {\n            // Emitimos evento para que el padre cierre el modal\n            this.$emit('close');\n\n            // Reseteamos pasos y campos\n            this.step = 1;\n            this.selectedTemplate = null;\n            this.maintenance = {\n                name: '',\n                type: '',\n                date: '',\n                km: '',\n                details: '',\n                intervalo: '',\n                email: '',\n                alerta: '',\n                fechaUltima: ''\n            };\n        },\n        saveMaintenance() {\n            console.log('--- saveMaintenance triggered ---');\n            console.log('maintenance actual:', this.maintenance);\n            console.log('selectedTemplate:', this.selectedTemplate);\n            console.log('selectedMantencion:', this.selectedMantencion);\n\n            if (!this.maintenance) return;\n\n            const nuevaMantencion = {\n                nombre: this.maintenance.name,\n                tipo: this.maintenance.type,\n                intervalo: Number(this.maintenance.intervalo || 0),\n                ultimo: this.selectedTemplate?.type === 'Por Odómetro'\n                    ? Number(this.maintenance.km || 0)\n                    : this.maintenance.fechaUltima,\n                actual: this.selectedTemplate?.type === 'Por Odómetro'\n                    ? Number(this.maintenance.km || 0)\n                    : new Date().toISOString().split('T')[0],\n                alerta: Number(this.maintenance.alerta || 0),\n                email: this.maintenance.email || '',\n                fechaCreacion: new Date().toISOString()\n            };\n\n            console.log('nuevaMantencion a emitir:', nuevaMantencion);\n\n            // Emitimos al padre\n            this.$emit('save', nuevaMantencion);\n\n            if (!this.selectedMantencion) {\n                this.selectedTemplate.used = true;\n            }\n\n            // Cerramos modal usando el método cancel\n            this.cancel();\n            console.log('Modal cerrado');\n        },\n\n\n        onlyNumber(e) {\n            const char = String.fromCharCode(e.keyCode)\n            if (!/[0-9]/.test(char)) e.preventDefault()\n        }\n    },\n\n    computed: {\n        ultimoCampoLabel() {\n            if (!this.selectedTemplate) return ''\n            return this.selectedTemplate.type === 'Por Odómetro'\n                ? 'Último Kilometraje'\n                : 'Fecha Última Mantención'\n        },\n        ultimoCampoValue: {\n            get() {\n                if (this.selectedTemplate?.type === 'Po