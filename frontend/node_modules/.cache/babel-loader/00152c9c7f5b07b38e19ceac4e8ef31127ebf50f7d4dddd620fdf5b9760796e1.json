{"ast":null,"code":"export default {\n  name: \"Login\",\n  data() {\n    return {\n      validCredentials: {\n        username: \"lightscope\",\n        password: \"lightscope\"\n      },\n      model: {\n        username: \"\",\n        password: \"\"\n      },\n      loading: false,\n      rules: {\n        username: [{\n          required: true,\n          message: \"Username is required\",\n          trigger: \"blur\"\n        }, {\n          min: 4,\n          message: \"Username must have at least 5 characters\",\n          trigger: \"blur\"\n        }],\n        password: [{\n          required: true,\n          message: \"Password is required\",\n          trigger: \"blur\"\n        }, {\n          min: 5,\n          message: \"Password must have at least 5 characters\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  methods: {\n    simulateLogin() {\n      return new Promise(resolve => setTimeout(resolve, 800));\n    },\n    async login() {\n      this.$refs.form.validate(async valid => {\n        if (!valid) return;\n        this.loading = true;\n        await this.simulateLogin();\n        this.loading = false;\n        if (this.model.username === this.validCredentials.username && this.model.password === this.validCredentials.password) {\n          this.$message.success(\"Login successful\");\n          // this.$router.push(\"/dashboard\");\n        } else {\n          this.$message.error(\"Invalid username or password\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","validCredentials","username","password","model","loading","rules","required","message","trigger","min","methods","simulateLogin","Promise","resolve","setTimeout","login","$refs","form","validate","valid","$message","success","error"],"sources":["/home/sebastian/quantum-fleet-core/frontend/src/Views/LoginAdmin.vue"],"sourcesContent":["<template>\n  <div class=\"flex items-center justify-center min-h-screen bg-gray-100 dark:bg-gray-900\">\n    <div class=\"bg-white dark:bg-gray-800 shadow-lg rounded-2xl p-8 w-[340px]\">\n      <h2 class=\"text-2xl font-semibold text-center mb-6 text-gray-700 dark:text-gray-100\">\n        Login\n      </h2>\n\n      <el-form\n        :model=\"model\"\n        :rules=\"rules\"\n        ref=\"form\"\n@submit.prevent=\"login\"\n\n        class=\"space-y-4\"\n      >\n        <!-- Username -->\n        <el-form-item prop=\"username\">\n          <el-input\n            v-model=\"model.username\"\n            placeholder=\"Username\"\n            prefix-icon=\"el-icon-user\"\n          ></el-input>\n        </el-form-item>\n\n        <!-- Password -->\n        <el-form-item prop=\"password\">\n          <el-input\n            v-model=\"model.password\"\n            placeholder=\"Password\"\n            type=\"password\"\n            prefix-icon=\"el-icon-lock\"\n          ></el-input>\n        </el-form-item>\n\n        <!-- Button -->\n        <el-form-item>\n          <el-button\n            :loading=\"loading\"\n            type=\"primary\"\n            native-type=\"submit\"\n            class=\"w-full bg-teal-600 hover:bg-teal-500 border-none text-white font-medium py-2 rounded-md transition-all\"\n          >\n            Login\n          </el-button>\n        </el-form-item>\n\n        <a\n          href=\"https://oxfordinformatics.com/\"\n          class=\"block text-center text-sm text-teal-600 hover:text-teal-500\"\n        >\n          Forgot password?\n        </a>\n      </el-form>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      validCredentials: {\n        username: \"lightscope\",\n        password: \"lightscope\",\n      },\n      model: {\n        username: \"\",\n        password: \"\",\n      },\n      loading: false,\n      rules: {\n        username: [\n          { required: true, message: \"Username is required\", trigger: \"blur\" },\n          {\n            min: 4,\n            message: \"Username must have at least 5 characters\",\n            trigger: \"blur\",\n          },\n        ],\n        password: [\n          { required: true, message: \"Password is required\", trigger: \"blur\" },\n          {\n            min: 5,\n            message: \"Password must have at least 5 characters\",\n            trigger: \"blur\",\n          },\n        ],\n      },\n    };\n  },\n  methods: {\n    simulateLogin() {\n      return new Promise((resolve) => setTimeout(resolve, 800));\n    },\n    async login() {\n      this.$refs.form.validate(async (valid) => {\n        if (!valid) return;\n\n        this.loading = true;\n        await this.simulateLogin();\n        this.loading = false;\n\n        if (\n          this.model.username === this.validCredentials.username &&\n          this.model.password === this.validCredentials.password\n        ) {\n          this.$message.success(\"Login successful\");\n          // this.$router.push(\"/dashboard\");\n        } else {\n          this.$message.error(\"Invalid username or password\");\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Ningún estilo custom — todo está en Tailwind */\n</style>\n"],"mappings":"AA0DA,eAAe;EACbA,IAAI,EAAE,OAAO;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,gBAAgB,EAAE;QAChBC,QAAQ,EAAE,YAAY;QACtBC,QAAQ,EAAE;MACZ,CAAC;MACDC,KAAK,EAAE;QACLF,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE;MACZ,CAAC;MACDE,OAAO,EAAE,KAAK;MACdC,KAAK,EAAE;QACLJ,QAAQ,EAAE,CACR;UAAEK,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,sBAAsB;UAAEC,OAAO,EAAE;QAAO,CAAC,EACpE;UACEC,GAAG,EAAE,CAAC;UACNF,OAAO,EAAE,0CAA0C;UACnDC,OAAO,EAAE;QACX,CAAC,CACF;QACDN,QAAQ,EAAE,CACR;UAAEI,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,sBAAsB;UAAEC,OAAO,EAAE;QAAO,CAAC,EACpE;UACEC,GAAG,EAAE,CAAC;UACNF,OAAO,EAAE,0CAA0C;UACnDC,OAAO,EAAE;QACX,CAAC;MAEL;IACF,CAAC;EACH,CAAC;EACDE,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAIC,OAAO,CAAEC,OAAO,IAAKC,UAAU,CAACD,OAAO,EAAE,GAAG,CAAC,CAAC;IAC3D,CAAC;IACD,MAAME,KAAKA,CAAA,EAAG;MACZ,IAAI,CAACC,KAAK,CAACC,IAAI,CAACC