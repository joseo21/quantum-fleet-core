{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport SvgIcon from '@/components/icons/SvgIcon.vue';\nexport default {\n  name: 'ModalVerMantenciones',\n  components: {\n    SvgIcon\n  },\n  props: {\n    patente: {\n      type: String,\n      required: true\n    },\n    mantenciones: {\n      type: Array,\n      required: true\n    },\n    horometrosActuales: {\n      type: Object,\n      default: () => ({})\n    },\n    odometroActual: {\n      type: Number,\n      default: 0\n    }\n  },\n  data() {\n    return {\n      currentPage: 1,\n      totalItemsPerPage: 3 // Máximo de 3 cards por página\n    };\n  },\n  watch: {\n    // Reinicia la página a 1 si la lista de mantenciones cambia (ej: si se filtra, añade o elimina)\n    mantenciones: {\n      handler() {\n        this.currentPage = 1;\n      },\n      deep: true\n    }\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      if (!fechaStr) return 'N/A';\n      const f = new Date(fechaStr);\n      if (isNaN(f.getTime())) return fechaStr;\n      const options = {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit'\n      };\n      return f.toLocaleString('es-CL', options);\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    }\n  },\n  computed: {\n    mantencionesConEstado() {\n      // Lógica de cálculo de estado (igual a la versión anterior, pero más limpia)\n      return this.mantenciones.map(m => {\n        const tipo = (m.tipo || m.type || '').toString().trim();\n        const intervalo = Number(m.intervalo || 0);\n        const alerta = Number(m.alerta || 0);\n        let umbral = null;\n        let faltante = null;\n        let estado = 'Desconocido';\n        let actual = 0;\n\n        // Odómetro\n        if (tipo === 'Por Odómetro') {\n          actual = Number(this.odometroActual || m.actual || 0);\n          const ultimo = Number(m.ultimo || 0);\n          const siguiente = ultimo + intervalo;\n          umbral = siguiente;\n          faltante = siguiente - actual;\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n        }\n\n        // Horómetro\n        else if (tipo === 'Por Horómetro') {\n          actual = this.horometrosActuales[this.patente] ?? Number(m.actual || 0);\n          const ultimo = Number(m.ultimo || 0);\n          const siguiente = ultimo + intervalo;\n          umbral = siguiente;\n          faltante = siguiente - actual;\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n        }\n\n        // Por Tiempo\n        else if (tipo === 'Por Tiempo') {\n          let ultimaFecha;\n          if (typeof m.ultimo === 'string' && m.ultimo.trim() !== '') {\n            const parts = m.ultimo.split('-');\n            if (parts.length === 3) {\n              if (parts[0].length === 4) {\n                // Formato YYYY-MM-DD\n                ultimaFecha = new Date(m.ultimo + 'T00:00:00');\n              } else {\n                // Asumir formato DD-MM-YYYY\n                const [dd, mm, yyyy] = parts;\n                ultimaFecha = new Date(`${yyyy}-${mm}-${dd}T00:00:00`);\n              }\n            } else {\n              ultimaFecha = new Date(m.ultimo);\n            }\n          } else {\n            ultimaFecha = new Date(0);\n          }\n          if (!isNaN(ultimaFecha.getTime()) && ultimaFecha.getTime() > 0) {\n            const umbralDate = new Date(ultimaFecha);\n            umbralDate.setDate(umbralDate.getDate() + intervalo);\n            const hoy = new Date();\n            const hoyNormalizado = new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate());\n            const diffTime = umbralDate.getTime() - hoyNormalizado.getTime();\n            faltante = Math.ceil(diffTime / (1000 * 60 * 60 * 24));\n            umbral = umbralDate.toISOString().split('T')[0];\n            if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n            actual = new Date().toISOString().split('T')[0];\n          } else {\n            umbral = 'N/A';\n            faltante = 'N/A';\n            actual = 'N/A';\n          }\n        }\n        return {\n          ...m,\n          umbral,\n          faltante,\n          estado,\n          actual\n        };\n      });\n    },\n    // CÁLCULO DE PAGINACIÓN\n    totalPages() {\n      // Calcula el número total de páginas\n      return Math.ceil(this.mantencionesConEstado.length / this.totalItemsPerPage);\n    },\n    mantencionesPaginadas() {\n      // Retorna solo los elementos de la página actual\n      const start = (this.currentPage - 1) * this.totalItemsPerPage;\n      const end = start + this.totalItemsPerPage;\n      return this.mantencionesConEstado.slice(start, end);\n    }\n  }\n};","map":{"version":3,"names":["SvgIcon","name","components","props","patente","type","String","required","mantenciones","Array","horometrosActuales","Object","default","odometroActual","Number","data","currentPage","totalItemsPerPage","watch","handler","deep","methods","formatFecha","fechaStr","f","Date","isNaN","getTime","options","year","month","day","hour","minute","toLocaleString","prevPage","nextPage","totalPages","computed","mantencionesConEstado","map","m","tipo","toString","trim","intervalo","alerta","umbral","faltante","estado","actual","ultimo","siguiente","ultimaFecha","parts","split","length","dd","mm","yyyy","umbralDate","setDate","getDate","hoy","hoyNormalizado","getFullYear","getMonth","diffTime","Math","ceil","toISOString","mantencionesPaginadas","start","end","slice"],"sources":["/home/sebastia