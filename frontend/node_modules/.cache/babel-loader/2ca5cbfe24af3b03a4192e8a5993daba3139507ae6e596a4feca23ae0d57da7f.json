or página según ancho de pantalla\n    this.updatePorPagina()\n    window.addEventListener('resize', this.updatePorPagina)\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.updatePorPagina)\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      const f = new Date(fechaStr)\n      return f.toLocaleString()\n    },\n    updatePorPagina() {\n      this.porPagina = window.innerWidth < 640 ? 1 : 6\n    },\n    nextPage() {\n      if (this.paginaActual < this.totalPaginas) this.paginaActual++\n    },\n    prevPage() {\n      if (this.paginaActual > 1) this.paginaActual--\n    }\n  }\n}\n</script>\n"],"mappings":";;;AAqHA,OAAOA,OAAM,MAAO,gCAA+B;AACnD,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAE;IAAEF;EAAQ,CAAC;EACvBG,KAAK,EAAE;IACLC,OAAO,EAAE;MAAEC,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACzCC,YAAY,EAAE;MAAEH,IAAI,EAAEI,KAAK;MAAEF,QAAQ,EAAE;IAAK,CAAC;IAC7CG,kBAAkB,EAAE;MAAEL,IAAI,EAAEM,MAAM;MAAEC,OAAO,EAAEA,CAAA,MAAO,CAAC,CAAC;IAAE,CAAC;IACzDC,cAAc,EAAE;MAAER,IAAI,EAAES,MAAM;MAAEF,OAAO,EAAE;IAAE;EAC7C,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;MACfC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,qBAAqBA,CAAA,EAAG;MACtB,OAAO,IAAI,CAACX,YAAY,CAACY,GAAG,CAAEC,CAAC,IAAK;QAClC,MAAMC,IAAG,GAAI,CAACD,CAAC,CAACC,IAAG,IAAKD,CAAC,CAAChB,IAAG,IAAK,EAAE,EAAEkB,QAAQ,CAAC,CAAC,CAACC,IAAI,CAAC;QACtD,MAAMC,SAAQ,GAAIX,MAAM,CAACO,CAAC,CAACI,SAAQ,IAAK,CAAC;QACzC,MAAMC,MAAK,GAAIZ,MAAM,CAACO,CAAC,CAACK,MAAK,IAAK,CAAC;QAEnC,IAAIC,MAAK,GAAI,IAAG;QAChB,IAAIC,QAAO,GAAI,IAAG;QAClB,IAAIC,MAAK,GAAI,aAAY;QACzB,IAAIC,MAAK,GAAI;QAEb,IAAIR,IAAG,KAAM,cAAc,EAAE;UAC3B,MAAMS,QAAO,GAAI,IAAI,CAACC,OAAO,CAACxB,YAAY,CAACyB,IAAI,CAACC,CAAA,IAAKA,CAAC,CAAC9B,OAAM,KAAM,IAAI,CAACA,OAAO;UAC/E0B,MAAK,GAAIC,QAAO,GAAIjB,MAAM,CAACiB,QAAQ,CAACI,QAAQ,IAAIrB,MAAM,CAACO,CAAC,CAACS,MAAK,IAAK,CAAC;UAEpE,MAAMM,MAAK,GAAItB,MAAM,CAACO,CAAC,CAACe,MAAK,IAAK,CAAC;UACnC,MAAMC,SAAQ,GAAID,MAAK,GAAIX,SAAQ;UACnCE,MAAK,GAAIU,SAAQ;UACjBT,QAAO,GAAIS,SAAQ,GAAIP,MAAK;UAE5B,IAAIF,QAAO,IAAK,CAAC,EAAEC,