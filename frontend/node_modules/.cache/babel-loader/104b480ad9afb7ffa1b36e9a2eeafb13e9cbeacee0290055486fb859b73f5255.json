        const siguiente = ultimo + intervalo\n                    umbral = siguiente\n                    faltante = siguiente - actual\n\n                    if (faltante <= 0) estado = 'Requiere mantención'\n                    else if (faltante <= alerta) estado = 'Próxima mantención'\n                    else estado = 'OK'\n                }\n\n                // Por Tiempo\n                else if (tipo === 'Por Tiempo') {\n                    let ultimaFecha\n                    if (typeof m.ultimo === 'string') {\n                        const parts = m.ultimo.split('-')\n                        if (parts.length === 3 && parts[0].length === 4) {\n                            // Formato YYYY-MM-DD\n                            ultimaFecha = new Date(m.ultimo)\n                        } else if (parts.length === 3) {\n                            // Formato DD-MM-YYYY\n                            const [dd, mm, yyyy] = parts\n                            ultimaFecha = new Date(Number(yyyy), Number(mm) - 1, Number(dd))\n                        } else {\n                            ultimaFecha = new Date(m.ultimo)\n                        }\n                    } else {\n                        ultimaFecha = new Date()\n                    }\n\n                    if (!isNaN(ultimaFecha.getTime())) {\n                        const umbralDate = new Date(ultimaFecha)\n                        umbralDate.setDate(umbralDate.getDate() + intervalo)\n                        const hoy = new Date()\n                        faltante = Math.ceil((umbralDate - new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate())) / (1000 * 60 * 60 * 24))\n                        umbral = umbralDate.toISOString().split('T')[0]\n\n                        if (faltante <= 0) estado = 'Requiere mantención'\n                        else if (faltante <= alerta) estado = 'Próxima mantención'\n                        else estado = 'OK'\n\n                        actual = new Date().toISOString().split('T')[0]\n                    }\n                }\n\n                return { ...m, umbral, faltante, estado, actual, horaRegistro }\n            })\n        }\n    }\n\n\n}\n</script>\n"],"mappings":";;;AAoGA,OAAOA,OAAM,MAAO,gCAA+B;AACnD,eAAe;EACXC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAE;IAAEF;EAAQ,CAAC;EACvBG,KAAK,EAAE;IACHC,OAAO,EAAE;MAAEC,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACzCC,YAAY,EAAE;MAAEH,IAAI,EAAEI,KAAK;MAAEF,QAAQ,EAAE;IAAK,CAAC;IAC7CG,kBAAkB,EAAE;MAAEL,IAAI,EAAEM,MAAM;MAAEC,OAAO,EAAEA,CAAA,MAAO,CAAC,CAAC;IAAE;EAC5D,CAAC;EACDC,OAAO,EAAE;IACLC,WAAWA,CAACC,QAAQ,EAAE;MAClB,MAAMC,CAAA,GAAI,IAAIC,IAAI,CAACF,QAAQ;MAC3B,OAAOC,CAAC,CAACE,cAAc,CAAC;IAC5B;EACJ,CAAC;EACDC,QAAQ,EAAE;IACNC,qBAAqBA,CAAA,EAAG;MACpB,OAAO,IAAI,CAACZ,YAAY,CAACa,GAAG,CAAEC,CAAC,IAAK;QAChC,MAAMC,IAAG,GAAI,CAACD,CAAC,CAACC,IAAG,IAAKD,CAAC,CAACjB,IAAG,IAAK,EAAE,EAAEmB,QAAQ,CAAC,CAAC,CAACC,IAAI,CAAC;QACtD,MAAMC,SAAQ,GAAIC,MAAM,CAACL,CAAC,CAACI,SAAQ,IAAK,CAAC;QACzC,MAAME,MAAK,GAAID,MAAM,CAACL,CAAC,CAACM,MAAK,IAAK,CAAC;QAEnC,IAAIC,MAAK,GAAI,IAAG;QAChB,IAAIC,QAAO,GAAI,IAAG;QAClB,IAAIC,MAAK,GAAI,aAAY;QACzB,IAAIC,MAAK,GAAI;QACb,IAAIC,YAAW,GAAIX,CAAC,CAACW,YAAW,IAAK,IAAG;;QAExC;QACA,IAAIV,IAAG,KAAM,cAAc,EAAE;UACzB;UACA,MAAMW,QAAO,GAAI,IAAI,CAACC,OAAO,CAAC3B,YAAY,CAAC4B,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACjC,OAAM,KAAM,IAAI,CAACA,OAAO;UAC/E4B,MAAK,GAAIE,QAAO,GAAIP,MAAM,CAACO,QAAQ,CAACI,QAAQ,IAAIX,MAAM,CAACL,CAAC,CAACU,MAAK,IAAK,CAAC;UAEpE,MAAMO,MAAK,GAAIZ,MAAM,CAACL,CAAC,CAACiB,MAAK,IAAK,CAAC;UACnC,MAAMC,SAAQ,GAAID,MAAK,GAAIb,SAAQ;UACnCG,MAAK,GAAIW,SAAQ;UACjBV,QAAO,GAAIU,SAAQ,GAAIR,MAAK;UAE5B,IAAIF,QAAO,IAAK,CAAC,EAAEC,MAAK,GAAI,qBAAoB,MAC3C,IAAID,QAAO,IAAKF,MAAM,EAAEG,MAAK,GAAI,oBAAmB,MACpDA,MAAK,GAAI,IAAG;QACrB;;QAEA;QAAA,KACK,IAAIR,IAAG,KAAM,eAAe,EAAE;UAC/BS,MAAK,GAAI,IAAI,CAACtB,kBAAkB,CAAC,IAAI,CAACN,OAAO,KAAKuB,MAAM,CAACL,CAAC,CAACU,MAAK,IAAK,CAAC;UAEtE,MAAMO,MAAK,GAAIZ,MAAM,CAACL,CAAC,CAACiB,MAAK,IAAK,CAAC;UACnC,MAAMC,SAAQ,GAAID,MAAK,GAAIb,SAAQ;UACnCG,MAAK,GAAIW,SAAQ;UACjBV,QAAO,GAAIU,SAAQ,GAAIR