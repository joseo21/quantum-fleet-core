{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport SvgIcon from '@/components/icons/SvgIcon.vue';\nimport { nextTick } from 'vue';\nexport default {\n  name: 'ModalAgregar',\n  components: {\n    SvgIcon\n  },\n  props: {\n    showAddModal: {\n      type: Boolean,\n      required: true\n    },\n    selectedPatente: {\n      type: String,\n      required: true\n    },\n    mantenciones: {\n      type: Array,\n      required: true\n    },\n    selectedMantencion: {\n      type: [Object, Number],\n      default: null\n    },\n    kmActual: {\n      type: Number,\n      default: 0\n    } // asegúrate de pasar esto desde padre\n  },\n  data() {\n    return {\n      step: 1,\n      templates: [{\n        id: 1,\n        name: 'Revisión Tecnica',\n        type: 'Por Tiempo',\n        description: 'Verificación completa del vehículo según normativa',\n        used: false\n      }, {\n        id: 2,\n        name: 'Permiso de Circulación',\n        type: 'Por Tiempo',\n        description: 'Gestión y pago del permiso de circulación anual',\n        used: false\n      }, {\n        id: 3,\n        name: 'Cambio de Neumáticos',\n        type: 'Por Odómetro',\n        description: 'Rotación, balanceo y revisión del desgaste de neumáticos',\n        used: false\n      }, {\n        id: 4,\n        name: 'Cambio de Aceite',\n        type: 'Por Odómetro',\n        description: 'Sustitución de aceite y filtro para mantener motor óptimo',\n        used: false\n      }, {\n        id: 5,\n        name: 'Cambio de Filtro de Aire',\n        type: 'Por Odómetro',\n        description: 'Reemplazo del filtro de aire para mejorar eficiencia del motor',\n        used: false\n      }],\n      selectedTemplate: null,\n      // estructura que usa el form (name/type son para inputs)\n      maintenance: {\n        name: '',\n        type: '',\n        date: '',\n        km: '',\n        details: '',\n        intervalo: '',\n        email: '',\n        alerta: '',\n        fechaUltima: ''\n      },\n      isModalButtonDisabled: false,\n      // guardamos la mantención original al entrar a editar para referencia\n      originalMantencion: null\n    };\n  },\n  methods: {\n    selectTemplate(template) {\n      // si está usado y estamos creando (no editando) bloqueamos\n      if (template.used && this.selectedMantencion === null) return;\n\n      // seleccionar\n      this.selectedTemplate = template;\n\n      // Si estamos creando (no editando), asignar nombre/tipo al form\n      if (this.selectedMantencion === null) {\n        this.maintenance.name = template.name;\n        this.maintenance.type = template.type;\n        // limpiar valores específicos\n        this.maintenance.km = '';\n        this.maintenance.fechaUltima = '';\n      } else {\n        // si estamos editando y cambiamos template, actualizar campos visibles\n        // si originalMantencion existe usamos su 'ultimo' para rellenar correctamente\n        if (this.originalMantencion) {\n          if (template.type === 'Por Odómetro') {\n            // convertir ultimo guardado a número si es posible\n            const val = Number(this.originalMantencion.ultimo);\n            this.maintenance.km = isNaN(val) ? '' : val;\n            this.maintenance.fechaUltima = ''; // limpiar campo fecha\n          } else {\n            // Por Tiempo\n            const val = this.originalMantencion.ultimo;\n            // si estaba almacenado como fecha (string) lo reutilizamos, si no, dejamos vacío\n            this.maintenance.fechaUltima = typeof val === 'string' ? val : '';\n            this.maintenance.km = '';\n          }\n        } else {\n          // fallback: asignar template name/type\n          this.maintenance.name = template.name;\n          this.maintenance.type = template.type;\n        }\n\n        // siempre sincronizar name/type con el template si el usuario cambia template\n        this.maintenance.name = template.name;\n        this.maintenance.type = template.type;\n      }\n    },\n    goToNextStep() {\n      if (this.selectedTemplate) this.step = 2;\n    },\n    cancel() {\n      this.$emit('close');\n      this.step = 1;\n      this.selectedTemplate = null;\n      this.originalMantencion = null;\n      this.maintenance = {\n        name: '',\n        type: '',\n        date: '',\n        km: '',\n        details: '',\n        intervalo: '',\n        email: '',\n        alerta: '',\n        fechaUltima: ''\n      };\n    },\n    saveMaintenance() {\n      // verificar mínimos\n      if (!this.maintenance) return;\n\n      // determinar tipo actual (priorizar maintenance.type, si no usar template)\n      const tipo = this.maintenance.type || this.selectedTemplate?.type || '';\n\n      // construir objeto normalizado que guardará el padre\n      const nuevaMantencion = {\n        nombre: this.maintenance.name || this.selectedTemplate?.name || '',\n        tipo: tipo,\n        intervalo: Number(this.maintenance.intervalo || 0),\n        alerta: Number(this.maintenance.alerta || 0),\n        email: this.maintenance.email || '',\n        fechaCreacion: new Date().toISOString()\n      };\n      if (tipo === 'Por Odómetro') {\n        // ultimo es km\n        nuevaMantencion.ultimo = Number(this.maintenance.km || 0);\n        // actual se toma del prop kmActual (padre debe enviarlo)\n        nuevaMantencion.actual = Number(this.kmActual || 0);\n      } else {\n        // Por Tiempo -> ultimo es fecha en formato ISO yyyy-mm-dd\n        nuevaMantencion.ultimo = this.maintenance.fechaUltima || new Date().toISOString().split('T')[0];\n        // actual puede guardarse como fecha de guardado\n        nuevaMantencion.actual = new Date().toISOString().split('T')[0];\n      }\n      console.log('[ModalAgregar] Emitting save with:', nuevaMantencion);\n      this.$emit('save', nuevaMantencion);\n\n      // marcar template como usado (si aplica)\n      if (this.selectedTemplate) {\n        const index = this.templates.findIndex(t => t.id === this.selectedTemplate.id);\n        if (index !== -1) this.templates[index].used = true;\n      }\n\n      // limpiar y cerrar\n      this.cancel();\n    },\n    onlyNumber(e) {\n      const char = String.fromCharCode(e.keyCode);\n      if (!/[0-9]/.test(char)) e.preventDefault();\n    }\n  },\n  computed: {\n    ultimoCampoLabel() {\n      if (!this.selectedTemplate) return '';\n      return this.selectedTemplate.type === 'Por Odómetro' ? 'Último Kilometraje' : 'Fecha Última Mantención';\n    },\n    ultimoCampoValue: {\n      get() {\n        return this.selectedTemplate?.type === 'Por Odómetro' ? this.maintenance.km : this.maintenance.fechaUltima;\n      },\n      set(value) {\n        if (this.selectedTemplate?.type === 'Por Odómetro') this.maintenance.km = value;else this.maintenance.fechaUltima = value;\n      }\n    },\n    templatesConEstado() {\n      const mantencionesVehiculo = this.mantenciones || [];\n      const nombresUsados = mantencionesVehiculo.filter((m, idx) => idx !== this.selectedMantencion).map(m => (m.nombre || m.name || '').toString().toLowerCase().trim());\n      return this.templates.map(t => ({\n        ...t,\n        used: nombresUsados.includes(t.name.toLowerCase())\n      }));\n    }\n  },\n  watch: {\n    // cuando cambie el índice de edición, cargamos correctamente el form\n    selectedMantencion: {\n      immediate: true,\n      async handler(val) {\n        if (val !== null && typeof val === 'number') {\n          const m = this.mantenciones[val];\n          if (m) {\n            // guardamos la mantención original para referencias futuras\n            this.originalMantencion = {\n              ...m\n            };\n\n            // buscar template por nombre (si existe)\n            this.selectedTemplate = this.templates.find(t => t.name.toLowerCase().trim() === (m.nombre || m.name || '').toString().toLowerCase().trim()) || null;\n\n            // esperar reactividad antes de setear campos que dependen del template\n            await nextTick();\n\n            // mapear m => maintenance (llaves usadas por el form)\n            this.maintenance = {\n              name: m.nombre || m.name || this.selectedTemplate?.name || '',\n              type: m.tipo || m.type || this.selectedTemplate?.type || '',\n              intervalo: m.intervalo || '',\n              alerta: m.alerta || '',\n              email: m.email || '',\n              details: m.details || '',\n              // completar km o fecha según el tipo detectado (priorizamos selectedTemplate)\n              km: this.selectedTemplate?.type === 'Por Odómetro' ? Number(m.ultimo) || '' : '',\n              fechaUltima: this.selectedTemplate?.type === 'Por Tiempo' ? typeof m.ultimo === 'string' ? m.ultimo : '' : ''\n            };\n            this.step = 2;\n          }\n        } else {\n          this.originalMantencion = null;\n          this.maintenance = {\n            name: '',\n            type: '',\n            date: '',\n            km: '',\n            details: '',\n            intervalo: '',\n            email: '',\n            alerta: '',\n            fechaUltima: ''\n          };\n          this.step = 1;\n          this.selectedTemplate = null;\n        }\n      }\n    },\n    selectedTemplate(newVal) {\n      if (!newVal) return;\n      if (this.originalMantencion && this.selectedMantencion !== null) {\n        const ultimo = this.originalMantencion.ultimo;\n        if (newVal.type === 'Por Odómetro') {\n          // ✅ usar valor actual real del vehículo, no el \"ultimo\"\n          const actualVehiculo = this.valorActualVehiculo ?? this.originalMantencion.actual ?? null;\n          this.maintenance.km = actualVehiculo || '';\n          this.maintenance.fechaUltima = '';\n        } else if (newVal.type === 'Por Tiempo') {\n          const fechaValida = typeof ultimo === 'string' && /^\\d{4}-\\d{2}-\\d{2}$/.test(ultimo);\n          this.maintenance.fechaUltima = fechaValida ? ultimo : '';\n          this.maintenance.km = '';\n        }\n        this.maintenance.name = newVal.name;\n        this.maintenance.type = newVal.type;\n      }\n    },\n    showAddModal(val) {\n      if (val) {\n        this.step = 1;\n        this.selectedTemplate = null;\n        this.originalMantencion = null;\n        nextTick(() => {\n          const nombresUsados = (this.mantenciones || []).map(m => (m.nombre || m.name || '').toString().trim().toLowerCase());\n          this.templates.forEach(t => t.used = nombresUsados.includes((t.name || '').toString().trim().toLowerCase()));\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["SvgIcon","nextTick","name","components","props","showAddModal","type","Boolean","required","selectedPatente","String","mantenciones","Array","selectedMantencion","Object","Number","default","kmActual","data","step","templates","id","description","used","selectedTemplate","maintenance","date","km","details","intervalo","email","alerta","fechaUltima","isModalButtonDisabled","originalMantencion","methods","selectTemplate","template","val","ultimo","isNaN","goToNextStep","cancel","$emit","saveMaintenance","tipo","nuevaMantencion","nombre","fechaCreacion","Date","toISOString","actual","split","console","log","index","findIndex","t","onlyNumber","e","char","fromCharCode","keyCode","test","preventDefault","computed","ultimoCampoLabel","ultimoCampoValue","get","set","value","templatesConEstado","mantencionesVehiculo","nombresUsados","filter","m","idx","map","toString","toLowerCase","trim","includes","watch","immediate","handler","find","newVal","actualVehiculo","valorActualVehiculo","fechaValida","forEach"],"sources":["/home/sebastian/quantum-fleet-core/frontend/src/components/MantencionesUi/ModalAgregar.vue"],"sourcesContent":["<template>\n  <!-- Modal multi-paso -->\n  <div v-if=\"showAddModal\"\n    class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-[60] p-2 sm:p-4\">\n    <div\n      class=\"bg-white dark:bg-gray-900 text-gray-900 dark:text-gray-100 rounded-lg relative flex flex-col w-full max-w-full sm:max-w-3xl h-auto p-4 sm:p-6 border border-gray-300 dark:border-gray-700 shadow-xl overflow-auto max-h-[90vh]\">\n      <button @click=\"$emit('close')\"\n        class=\"absolute top-4 right-4 p-2 rounded hover:bg-gray-200 dark:hover:bg-gray-700 transition\" title=\"Cerrar\">\n        <SvgIcon name=\"close\" class=\"w-6 h-6\" />\n      </button>\n      <!-- Botón volver paso 2 -> 1 -->\n      <button v-if=\"step === 2\" @click=\"step = 1\"\n        class=\"absolute top-4 left-4 p-2 rounded hover:bg-gray-200 dark:hover:bg-gray-700 transition\" title=\"Volver\">\n        <SvgIcon name=\"arrow-left\" class=\"w-6 h-6\" />\n      </button>\n\n      <h2 class=\"text-2xl font-bold mb-4 text-center text-[#102372] dark:text-[#ff6600]\">\n        {{ step === 1 ? \"Seleccionar Template\" : `Agregar Mantención Vehículo (${selectedPatente})` }}\n      </h2>\n\n      <span class=\"h-px bg-gray-300 dark:bg-gray-700 mb-4\"></span>\n\n      <!-- Paso 1: Seleccionar Template -->\n      <div v-if=\"step === 1\" class=\"flex flex-col items-center gap-4 py-4 w-full\">\n        <div class=\"flex justify-center gap-4 flex-wrap w-full\">\n          <div v-for=\"template in templatesConEstado\" :key=\"template.id\" @click=\"selectTemplate(template)\" :class=\"[\n            'relative p-4 sm:p-6 w-full sm:w-48 rounded-lg border cursor-pointer text-center transition-all',\n            template.used\n              ? 'border-gray-500 bg-gray-200 dark:bg-gray-800 opacity-60 cursor-not-allowed'\n              : selectedTemplate?.id === template.id\n                ? 'border-[#ff6600] bg-orange-50 dark:bg-[#ff6600]/20 dark:border-[#ff6600]'\n                : 'border-gray-300 dark:border-gray-600 hover:bg-gray-100 dark:hover:bg-gray-800'\n          ]\">\n\n            <h3 class=\"text-lg font-semibold mb-2\">{{ template.name }}</h3>\n            <p class=\"text-gray-600 dark:text-gray-300 text-sm mb-2\">{{ template.description }}</p>\n\n            <div v-if=\"template.used\"\n              class=\"absolute inset-0 flex items-center justify-center bg-black bg-opacity-50 text-white rounded-lg text-sm font-semibold\">\n              Este vehículo ya tiene este tipo de mantención\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Paso 2: Formulario -->\n      <form v-else @submit.prevent=\"saveMaintenance\"\n        class=\"grid grid-cols-1 sm:grid-cols-2 gap-4 py-2 overflow-y-auto w-full\">\n\n        <div>\n          <label class=\"block text-sm mb-1\">Nombre de la Mantención</label>\n          <input type=\"text\"\n            class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n            v-model=\"maintenance.name\" disabled />\n        </div>\n\n        <div>\n          <label class=\"block text-sm mb-1\">Tipo de Mantención</label>\n          <input type=\"text\"\n            class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n            v-model=\"maintenance.type\" disabled />\n        </div>\n\n        <div>\n          <label class=\"block text-sm mb-1\">Intervalo</label>\n          <input type=\"number\" placeholder=\"Ej: 30 días o 5000 km\"\n            class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n            v-model=\"maintenance.intervalo\" @keypress=\"onlyNumber($event)\" />\n        </div>\n\n        <div>\n          <label class=\"block text-sm mb-1\">Email de contacto</label>\n          <input type=\"email\" placeholder=\"Ej: contacto@empresa.cl\"\n            class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n            v-model=\"maintenance.email\" />\n        </div>\n\n        <div>\n          <label class=\"block text-sm mb-1\">Alerta</label>\n          <input type=\"number\" placeholder=\"Ej: 5 días antes o 200 km antes\"\n            class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n            v-model=\"maintenance.alerta\" @keypress=\"onlyNumber($event)\" />\n        </div>\n\n        <div>\n          <label class=\"block text-sm mb-1\">{{ ultimoCampoLabel }}</label>\n          <input v-model=\"ultimoCampoValue\" :type=\"selectedTemplate?.type === 'Por Odómetro' ? 'number' : 'date'\"\n            placeholder=\"Ej: 45000\"\n            class=\"w-full p-2 rounded-md bg-gray-100 dark:bg-gray-800 text-gray-900 dark:text-white border border-gray-300 dark:border-gray-600\"\n            @keypress=\"selectedTemplate?.type === 'Por Odómetro' ? onlyNumber($event) : null\" />\n        </div>\n\n      </form>\n\n      <!-- Botones del modal -->\n      <div class=\"flex flex-col sm:flex-row justify-between mt-6 gap-2 w-full\">\n        <button @click=\"cancel\"\n          class=\"w-full sm:w-[48%] bg-gray-300 dark:bg-gray-700 hover:bg-gray-400 dark:hover:bg-gray-600 text-gray-900 dark:text-white px-4 py-2 rounded transition\">\n          Cancelar\n        </button>\n\n        <button v-if=\"step === 1\" @click=\"goToNextStep\" :disabled=\"!selectedTemplate\" :class=\"[\n          'w-full sm:w-[48%] px-4 py-2 rounded font-semibold transition',\n          selectedTemplate\n            ? 'bg-[#102372] hover:bg-[#0d1c5a] text-white'\n            : 'bg-[#102372] opacity-50 cursor-not-allowed text-white'\n        ]\">\n          Siguiente\n        </button>\n\n        <button v-else type=\"button\" @click=\"saveMaintenance\" :disabled=\"isModalButtonDisabled\" :class=\"[\n          'w-full sm:w-[48%] px-4 py-2 rounded font-semibold transition',\n          isModalButtonDisabled\n            ? 'bg-[#102372] opacity-50 cursor-not-allowed text-white'\n            : 'bg-[#102372] hover:bg-[#0d1c5a] text-white'\n        ]\">\n          Guardar\n        </button>\n      </div>\n\n    </div>\n  </div>\n</template>\n<script>\nimport SvgIcon from '@/components/icons/SvgIcon.vue'\nimport { nextTick } from 'vue'\nexport default {\n  name: 'ModalAgregar',\n  components: { SvgIcon },\n  props: {\n    showAddModal: { type: Boolean, required: true },\n    selectedPatente: { type: String, required: true },\n    mantenciones: { type: Array, required: true },\n    selectedMantencion: { type: [Object, Number], default: null },\n    kmActual: { type: Number, default: 0 } // asegúrate de pasar esto desde padre\n  },\n  data() {\n    return {\n      step: 1,\n      templates: [\n        { id: 1, name: 'Revisión Tecnica', type: 'Por Tiempo', description: 'Verificación completa del vehículo según normativa', used: false },\n        { id: 2, name: 'Permiso de Circulación', type: 'Por Tiempo', description: 'Gestión y pago del permiso de circulación anual', used: false },\n        { id: 3, name: 'Cambio de Neumáticos', type: 'Por Odómetro', description: 'Rotación, balanceo y revisión del desgaste de neumáticos', used: false },\n        { id: 4, name: 'Cambio de Aceite', type: 'Por Odómetro', description: 'Sustitución de aceite y filtro para mantener motor óptimo', used: false },\n        { id: 5, name: 'Cambio de Filtro de Aire', type: 'Por Odómetro', description: 'Reemplazo del filtro de aire para mejorar eficiencia del motor', used: false },\n      ],\n      selectedTemplate: null,\n      // estructura que usa el form (name/type son para inputs)\n      maintenance: { name: '', type: '', date: '', km: '', details: '', intervalo: '', email: '', alerta: '', fechaUltima: '' },\n      isModalButtonDisabled: false,\n      // guardamos la mantención original al entrar a editar para referencia\n      originalMantencion: null\n    }\n  },\n  methods: {\n    selectTemplate(template) {\n      // si está usado y estamos creando (no editando) bloqueamos\n      if (template.used && this.selectedMantencion === null) return\n\n      // seleccionar\n      this.selectedTemplate = template\n\n      // Si estamos creando (no editando), asignar nombre/tipo al form\n      if (this.selectedMantencion === null) {\n        this.maintenance.name = template.name\n        this.maintenance.type = template.type\n        // limpiar valores específicos\n        this.maintenance.km = ''\n        this.maintenance.fechaUltima = ''\n      } else {\n        // si estamos editando y cambiamos template, actualizar campos visibles\n        // si originalMantencion existe usamos su 'ultimo' para rellenar correctamente\n        if (this.originalMantencion) {\n          if (template.type === 'Por Odómetro') {\n            // convertir ultimo guardado a número si es posible\n            const val = Number(this.originalMantencion.ultimo)\n            this.maintenance.km = isNaN(val) ? '' : val\n            this.maintenance.fechaUltima = '' // limpiar campo fecha\n          } else {\n            // Por Tiempo\n            const val = this.originalMantencion.ultimo\n            // si estaba almacenado como fecha (string) lo reutilizamos, si no, dejamos vacío\n            this.maintenance.fechaUltima = typeof val === 'string' ? val : ''\n            this.maintenance.km = ''\n          }\n        } else {\n          // fallback: asignar template name/type\n          this.maintenance.name = template.name\n          this.maintenance.type = template.type\n        }\n\n        // siempre sincronizar name/type con el template si el usuario cambia template\n        this.maintenance.name = template.name\n        this.maintenance.type = template.type\n      }\n    },\n\n    goToNextStep() {\n      if (this.selectedTemplate) this.step = 2\n    },\n\n    cancel() {\n      this.$emit('close')\n      this.step = 1\n      this.selectedTemplate = null\n      this.originalMantencion = null\n      this.maintenance = { name: '', type: '', date: '', km: '', details: '', intervalo: '', email: '', alerta: '', fechaUltima: '' }\n    },\n\n    saveMaintenance() {\n      // verificar mínimos\n      if (!this.maintenance) return\n\n      // determinar tipo actual (priorizar maintenance.type, si no usar template)\n      const tipo = this.maintenance.type || this.selectedTemplate?.type || ''\n\n      // construir objeto normalizado que guardará el padre\n      const nuevaMantencion = {\n        nombre: this.maintenance.name || this.selectedTemplate?.name || '',\n        tipo: tipo,\n        intervalo: Number(this.maintenance.intervalo || 0),\n        alerta: Number(this.maintenance.alerta || 0),\n        email: this.maintenance.email || '',\n        fechaCreacion: new Date().toISOString()\n      }\n\n      if (tipo === 'Por Odómetro') {\n        // ultimo es km\n        nuevaMantencion.ultimo = Number(this.maintenance.km || 0)\n        // actual se toma del prop kmActual (padre debe enviarlo)\n        nuevaMantencion.actual = Number(this.kmActual || 0)\n      } else {\n        // Por Tiempo -> ultimo es fecha en formato ISO yyyy-mm-dd\n        nuevaMantencion.ultimo = this.maintenance.fechaUltima || new Date().toISOString().split('T')[0]\n        // actual puede guardarse como fecha de guardado\n        nuevaMantencion.actual = new Date().toISOString().split('T')[0]\n      }\n\n      console.log('[ModalAgregar] Emitting save with:', nuevaMantencion)\n      this.$emit('save', nuevaMantencion)\n\n      // marcar template como usado (si aplica)\n      if (this.selectedTemplate) {\n        const index = this.templates.findIndex(t => t.id === this.selectedTemplate.id)\n        if (index !== -1) this.templates[index].used = true\n      }\n\n      // limpiar y cerrar\n      this.cancel()\n    },\n\n    onlyNumber(e) { const char = String.fromCharCode(e.keyCode); if (!/[0-9]/.test(char)) e.preventDefault() }\n  },\n\n  computed: {\n    ultimoCampoLabel() { if (!this.selectedTemplate) return ''; return this.selectedTemplate.type === 'Por Odómetro' ? 'Último Kilometraje' : 'Fecha Última Mantención' },\n    ultimoCampoValue: {\n      get() { return this.selectedTemplate?.type === 'Por Odómetro' ? this.maintenance.km : this.maintenance.fechaUltima },\n      set(value) { if (this.selectedTemplate?.type === 'Por Odómetro') this.maintenance.km = value; else this.maintenance.fechaUltima = value }\n    },\n    templatesConEstado() {\n      const mantencionesVehiculo = this.mantenciones || []\n      const nombresUsados = mantencionesVehiculo\n        .filter((m, idx) => idx !== this.selectedMantencion)\n        .map(m => ((m.nombre || m.name) || '').toString().toLowerCase().trim())\n\n      return this.templates.map(t => ({ ...t, used: nombresUsados.includes(t.name.toLowerCase()) }))\n    }\n  },\n\n  watch: {\n    // cuando cambie el índice de edición, cargamos correctamente el form\n    selectedMantencion: {\n      immediate: true,\n      async handler(val) {\n        if (val !== null && typeof val === 'number') {\n          const m = this.mantenciones[val]\n          if (m) {\n            // guardamos la mantención original para referencias futuras\n            this.originalMantencion = { ...m }\n\n            // buscar template por nombre (si existe)\n            this.selectedTemplate = this.templates.find(\n              t => t.name.toLowerCase().trim() === (m.nombre || m.name || '').toString().toLowerCase().trim()\n            ) || null\n\n            // esperar reactividad antes de setear campos que dependen del template\n            await nextTick()\n\n            // mapear m => maintenance (llaves usadas por el form)\n            this.maintenance = {\n              name: m.nombre || m.name || this.selectedTemplate?.name || '',\n              type: m.tipo || m.type || this.selectedTemplate?.type || '',\n              intervalo: m.intervalo || 