{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport SvgIcon from '@/components/icons/SvgIcon.vue';\nexport default {\n  name: 'ModalVerMantenciones',\n  components: {\n    SvgIcon\n  },\n  props: {\n    patente: {\n      type: String,\n      required: true\n    },\n    mantenciones: {\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      currentPage: 1,\n      cardsPerPage: 3 // por defecto, se ajusta automáticamente según pantalla\n    };\n  },\n  computed: {\n    mantencionesConEstado() {\n      return this.mantenciones.map(m => {\n        const tipo = (m.tipo || m.type || '').toString().trim();\n        const intervalo = Number(m.intervalo || 0);\n        const alerta = Number(m.alerta || 0);\n        let umbral = null,\n          faltante = null,\n          estado = 'Desconocido';\n        if (tipo === 'Por Odómetro') {\n          const ultimo = Number(m.ultimo || 0);\n          const actual = Number(m.actual || 0);\n          const siguiente = ultimo + intervalo;\n          umbral = siguiente;\n          faltante = siguiente - actual;\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n        } else if (tipo === 'Por Tiempo') {\n          let ultimaFecha;\n          if (typeof m.ultimo === 'string' && m.ultimo.includes('-')) {\n            const parts = m.ultimo.split('-');\n            if (parts.length === 3 && parts[0].length === 4) ultimaFecha = new Date(m.ultimo);else if (parts.length === 3) {\n              const [dd, mm, yyyy] = parts;\n              ultimaFecha = new Date(Number(yyyy), Number(mm) - 1, Number(dd));\n            } else ultimaFecha = new Date(m.ultimo);\n          } else ultimaFecha = new Date();\n          if (!isNaN(ultimaFecha.getTime())) {\n            const umbralDate = new Date(ultimaFecha);\n            umbralDate.setDate(umbralDate.getDate() + intervalo);\n            const hoy = new Date();\n            faltante = Math.ceil((umbralDate - new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate())) / (1000 * 60 * 60 * 24));\n            umbral = umbralDate.toISOString().split('T')[0];\n            if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n          }\n        }\n        return {\n          ...m,\n          umbral,\n          faltante,\n          estado\n        };\n      });\n    },\n    paginatedCards() {\n      const start = (this.currentPage - 1) * this.cardsPerPage;\n      return this.mantencionesConEstado.slice(start, start + this.cardsPerPage);\n    },\n    totalPages() {\n      return Math.ceil(this.mantencionesConEstado.length / this.cardsPerPage);\n    }\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      const f = new Date(fechaStr);\n      return f.toLocaleString();\n    },\n    prevPage() {\n      if (this.currentPage > 1) this.currentPage--;\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) this.currentPage++;\n    },\n    updateCardsPerPage() {\n      const width = window.innerWidth;\n      if (width < 640) this.cardsPerPage = 1;else if (width < 1024) this.cardsPerPage = 2;else this.cardsPerPage = 3;\n    }\n  },\n  mounted() {\n    this.updateCardsPerPage();\n    window.addEventListener('resize', this.updateCardsPerPage);\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.updateCardsPerPage);\n  }\n};","map":{"version":3,"names":["SvgIcon","name","components","props","patente","type","String","required","mantenciones","Array","data","currentPage","cardsPerPage","computed","mantencionesConEstado","map","m","tipo","toString","trim","intervalo","Number","alerta","umbral","faltante","estado","ultimo","actual","siguiente","ultimaFecha","includes","parts","split","length","Date","dd","mm","yyyy","isNaN","getTime","umbralDate","setDate","getDate","hoy","Math","ceil","getFullYear","getMonth","toISOString","paginatedCards","start","slice","totalPages","methods","formatFecha","fechaStr","f","toLocaleString","prevPage","nextPage","updateCardsPerPage","width","window","innerWidth","mounted","addEventListener","beforeUnmount","removeEventListener"],"sources":["/home/sebastian/quantum-fleet-core/frontend/src/components/MantencionesUi/ModalVerMantenciones.vue"],"sourcesContent":["<template>\n  <div class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-2 sm:p-4 overflow-auto\">\n    <div class=\"bg-white dark:bg-gray-900 text-gray-900 dark:text-gray-100 rounded-lg relative w-full max-w-5xl max-h-[90vh] p-4 sm:p-6 border border-gray-300 dark:border-gray-700 shadow-xl overflow-hidden\">\n\n      <!-- Header -->\n      <h2 class=\"text-xl sm:text-2xl font-bold mb-4 text-center text-[#102372] dark:text-[#ff6600]\">\n        Mantenciones Vehículo {{ patente }}\n      </h2>\n\n      <!-- Botón cerrar -->\n      <button @click=\"$emit('close')\"\n        class=\"absolute top-2 right-2 sm:top-4 sm:right-4 p-1 sm:p-2 rounded hover:bg-gray-200 dark:hover:bg-gray-700 transition z-50\"\n        title=\"Cerrar\">\n        <SvgIcon name=\"close\" class=\"w-5 h-5 sm:w-6 sm:h-6\" />\n      </button>\n\n      <!-- Carrusel de cards -->\n      <div class=\"flex flex-col items-center gap-4\">\n        <div class=\"flex gap-4 flex-wrap justify-center\">\n          <div v-for=\"(m, idx) in paginatedCards\" :key=\"idx\"\n            class=\"bg-gray-100 dark:bg-gray-800 rounded-lg shadow p-4 flex flex-col gap-2 border border-gray-300 dark:border-gray-700 text-sm w-full sm:w-[calc(33%-0.5rem)]\">\n            <!-- Card Content -->\n            <div class=\"flex justify-between items-center mb-2\">\n              <span class=\"font-semibold\">{{ m.nombre || m.name }}</span>\n              <span class=\"text-xs px-2 py-1 rounded bg-blue-600 text-white\">{{ m.tipo || m.type }}</span>\n            </div>\n\n            <div class=\"grid grid-cols-2 sm:grid-cols-3 gap-2\">\n              <div>\n                <span class=\"text-gray-500 text-xs\">Intervalo</span>\n                <div class=\"font-medium\">{{ m.intervalo }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Último</span>\n                <div class=\"font-medium\">{{ m.ultimo }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Umbral</span>\n                <div class=\"font-medium\">{{ m.umbral }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Actual</span>\n                <div class=\"font-medium\">{{ m.actual }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Faltante</span>\n                <div class=\"font-medium\">{{ m.faltante }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Alerta</span>\n                <div class=\"font-medium\">{{ m.alerta || 0 }}</div>\n              </div>\n              <div class=\"sm:col-span-2\">\n                <span class=\"text-gray-500 text-xs\">Email</span>\n                <div class=\"font-medium\">{{ m.email || m.correo || 'N/A' }}</div>\n              </div>\n              <div class=\"sm:col-span-2\">\n                <span class=\"text-gray-500 text-xs\">Creación</span>\n                <div class=\"font-medium\">{{ formatFecha(m.fechaCreacion) }}</div>\n              </div>\n            </div>\n\n            <!-- Estado e imágenes -->\n            <div class=\"flex items-center justify-between mt-2\">\n              <div>\n                <img v-if=\"m.estado === 'OK'\" src=\"https://sinergygroup.cl/Mantenciones/img/verde.png\"\n                  class=\"w-12 h-12 object-contain\" />\n                <img v-else-if=\"m.estado === 'Próxima mantención'\" src=\"https://sinergygroup.cl/Mantenciones/img/amarillo.png\"\n                  class=\"w-12 h-12 object-contain\" />\n                <img v-else-if=\"m.estado === 'Requiere mantención'\" src=\"https://sinergygroup.cl/Mantenciones/img/rojo.png\"\n                  class=\"w-12 h-12 object-contain\" />\n              </div>\n\n              <!-- Botones -->\n              <div class=\"flex gap-2\">\n                <button @click=\"$emit('editar', idx)\"\n                  class=\"bg-yellow-500 hover:bg-yellow-600 text-white px-3 py-1 rounded text-sm\">Editar</button>\n                <button @click=\"$emit('eliminar', idx)\"\n                  class=\"bg-red-500 hover:bg-red-600 text-white px-3 py-1 rounded text-sm\">Eliminar</button>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- Paginación -->\n        <div v-if=\"totalPages > 1\" class=\"flex gap-3 mt-3 items-center\">\n          <button @click=\"prevPage\" :disabled=\"currentPage === 1\"\n            class=\"px-3 py-1 rounded border border-gray-400 dark:border-gray-600 disabled:opacity-40 hover:bg-[#ff6600] hover:text-white transition\">\n            Anterior\n          </button>\n          <span>Página {{ currentPage }} / {{ totalPages }}</span>\n          <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\"\n            class=\"px-3 py-1 rounded border border-gray-400 dark:border-gray-600 disabled:opacity-40 hover:bg-[#ff6600] hover:text-white transition\">\n            Siguiente\n          </button>\n        </div>\n\n        <!-- No hay mantenciones -->\n        <div v-if=\"!mantenciones || mantenciones.length === 0\"\n          class=\"text-center py-6 text-gray-500 dark:text-gray-400\">\n          No hay mantenciones registradas\n        </div>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport SvgIcon from '@/components/icons/SvgIcon.vue'\n\nexport default {\n  name: 'ModalVerMantenciones',\n  components: { SvgIcon },\n  props: {\n    patente: { type: String, required: true },\n    mantenciones: { type: Array, required: true }\n  },\n  data() {\n    return {\n      currentPage: 1,\n      cardsPerPage: 3 // por defecto, se ajusta automáticamente según pantalla\n    }\n  },\n  computed: {\n    mantencionesConEstado() {\n      return this.mantenciones.map((m) => {\n        const tipo = (m.tipo || m.type || '').toString().trim()\n        const intervalo = Number(m.intervalo || 0)\n        const alerta = Number(m.alerta || 0)\n        let umbral = null, faltante = null, estado = 'Desconocido'\n\n        if (tipo === 'Por Odómetro') {\n          const ultimo = Number(m.ultimo || 0)\n          const actual = Number(m.actual || 0)\n          const siguiente = ultimo + intervalo\n          umbral = siguiente\n          faltante = siguiente - actual\n          if (faltante <= 0) estado = 'Requiere mantención'\n          else if (faltante <= alerta) estado = 'Próxima mantención'\n          else estado = 'OK'\n        } else if (tipo === 'Por Tiempo') {\n          let ultimaFecha\n          if (typeof m.ultimo === 'string' && m.ultimo.includes('-')) {\n            const parts = m.ultimo.split('-')\n            if (parts.length === 3 && parts[0].length === 4) ultimaFecha = new Date(m.ultimo)\n            else if (parts.length === 3) {\n              const [dd, mm, yyyy] = parts\n              ultimaFecha = new Date(Number(yyyy), Number(mm)-1, Number(dd))\n            } else ultimaFecha = new Date(m.ultimo)\n          } else ultimaFecha = new Date()\n          if (!isNaN(ultimaFecha.getTime())) {\n            const umbralDate = new Date(ultimaFecha)\n            umbralDate.setDate(umbralDate.getDate() + intervalo)\n            const hoy = new Date()\n            faltante = Math.ceil((umbralDate - new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate()))/(1000*60*60*24))\n            umbral = umbralDate.toISOString().split('T')[0]\n            if (faltante <= 0) estado = 'Requiere mantención'\n            else if (faltante <= alerta) estado = 'Próxima mantención'\n            else estado = 'OK'\n          }\n        }\n        \n        return {...m, umbral, faltante, estado}\n      })\n    },\n    paginatedCards() {\n      const start = (this.currentPage -1) * this.cardsPerPage\n      return this.mantencionesConEstado.slice(start, start + this.cardsPerPage)\n    },\n    totalPages() {\n      return Math.ceil(this.mantencionesConEstado.length / this.cardsPerPage)\n    }\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      const f = new Date(fechaStr)\n      return f.toLocaleString()\n    },\n    prevPage() {\n      if (this.currentPage > 1) this.currentPage--\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) this.currentPage++\n    },\n    updateCardsPerPage() {\n      const width = window.innerWidth\n      if (width < 640) this.cardsPerPage = 1\n      else if (width < 1024) this.cardsPerPage = 2\n      else this.cardsPerPage = 3\n    }\n  },\n  mounted() {\n    this.updateCardsPerPage()\n    window.addEventListener('resize', this.updateCardsPerPage)\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.updateCardsPerPage)\n  }\n}\n</script>\n"],"mappings":";;AA6GA,OAAOA,OAAM,MAAO,gCAA+B;AAEnD,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAE;IAAEF;EAAQ,CAAC;EACvBG,KAAK,EAAE;IACLC,OAAO,EAAE;MAAEC,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACzCC,YAAY,EAAE;MAAEH,IAAI,EAAEI,KAAK;MAAEF,QAAQ,EAAE;IAAK;EAC9C,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE,EAAE;IAClB;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,qBAAqBA,CAAA,EAAG;MACtB,OAAO,IAAI,CAACN,YAAY,CAACO,GAAG,CAAEC,CAAC,IAAK;QAClC,MAAMC,IAAG,GAAI,CAACD,CAAC,CAACC,IAAG,IAAKD,CAAC,CAACX,IAAG,IAAK,EAAE,EAAEa,QAAQ,CAAC,CAAC,CAACC,IAAI,CAAC;QACtD,MAAMC,SAAQ,GAAIC,MAAM,CAACL,CAAC,CAACI,SAAQ,IAAK,CAAC;QACzC,MAAME,MAAK,GAAID,MAAM,CAACL,CAAC,CAACM,MAAK,IAAK,CAAC;QACnC,IAAIC,MAAK,GAAI,IAAI;UAAEC,QAAO,GAAI,IAAI;UAAEC,MAAK,GAAI,aAAY;QAEzD,IAAIR,IAAG,KAAM,cAAc,EAAE;UAC3B,MAAMS,MAAK,GAAIL,MAAM,CAACL,CAAC,CA