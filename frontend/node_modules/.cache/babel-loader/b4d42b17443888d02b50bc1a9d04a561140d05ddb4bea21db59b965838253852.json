ual || m.actual || 0);\n          const ultimo = Number(m.ultimo || 0);\n          const siguiente = ultimo + intervalo;\n          umbral = siguiente;\n          faltante = siguiente - actual;\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n        } else if (tipo === 'Por Horómetro') {\n          actual = this.horometrosActuales[this.patente] ?? Number(m.actual || 0);\n          const ultimo = Number(m.ultimo || 0);\n          const siguiente = ultimo + intervalo;\n          umbral = siguiente;\n          faltante = siguiente - actual;\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n        } else if (tipo === 'Por Tiempo') {\n          let ultimaFecha;\n          if (typeof m.ultimo === 'string' && m.ultimo.trim() !== '') {\n            const parts = m.ultimo.split('-');\n            if (parts.length === 3) {\n              if (parts[0].length === 4) {\n                ultimaFecha = new Date(m.ultimo + 'T00:00:00');\n              } else {\n                const [dd, mm, yyyy] = parts;\n                ultimaFecha = new Date(`${yyyy}-${mm}-${dd}T00:00:00`);\n              }\n            } else {\n              ultimaFecha = new Date(m.ultimo);\n            }\n          } else {\n            ultimaFecha = new Date(0);\n          }\n          if (!isNaN(ultimaFecha.getTime()) && ultimaFecha.getTime() > 0) {\n            const umbralDate = new Date(ultimaFecha);\n            umbralDate.setDate(umbralDate.getDate() + intervalo);\n            const hoy = new Date();\n            const hoyNormalizado = new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate());\n            const diffTime = umbralDate.getTime() - hoyNormalizado.getTime();\n            faltante = Math.ceil(diffTime / (1000 * 60 * 60 * 24));\n            umbral = umbralDate.toISOString().split('T')[0];\n            if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n            actual = new Date().toISOString().split('T')[0];\n          } else {\n            umbral = 'N/A';\n            faltante = 'N/A';\n            actual = 'N/A';\n          }\n        }\n        return {\n          ...m,\n          umbral,\n          faltante,\n          estado,\n          actual\n        };\n      });\n    },\n    // CÁLCULO DE PAGINACIÓN\n    totalPages() {\n      return Math.ceil(this.mantencionesConEstado.length / this.totalItemsPerPage);\n    },\n    mantencionesPaginadas() {\n      const start = (this.currentPage - 1) * this.totalItemsPerPage;\n      const end = start + this.totalItemsPerPage;\n      return this.mantencionesConEstado.slice(start, end);\n    }\n  }\n};","map":{"version":3,"names":["SvgIcon","name","components","props","patente","type","String","required","mantenciones","Array","horometrosActuales","Object","default","odometroActual","Number","data","currentPage","totalItemsPerPage","watch","handler","deep","methods","formatFecha","fechaStr","f","Date","isNaN","getTime","options","year","month","day","hour","minute","toLocaleString","prevPage","nextPage","totalPages","computed","mantencionesConEstado","map","m","tipo","toString","trim","intervalo","alerta","umbral","faltante","estado","actual","ultimo","siguiente","ultimaFecha","parts","split","length","dd","mm","yyyy","umbralDate","setDate","getDate","hoy","hoyNormalizado","getFullYear","getMonth","diffTime","Math","ceil","toISOString","mantencionesPaginadas","start","end","slice"],"sources":["/home/sebastian/quantum-fleet-core/frontend/src/components/MantencionesUi/ModalVerMantenciones.vue"],"sourcesContent":["<template>\n  <div class=\"fixed inset-0 bg-black bg-opacity-70 flex items-center justify-center z-50 p-3 sm:p-5 overflow-y-auto\">\n    <div\n      class=\"bg-white dark:bg-gray-800 text-gray-900 dark:text-gray-100 rounded-2xl relative w-fu