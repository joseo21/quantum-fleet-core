{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport SvgIcon from '@/components/icons/SvgIcon.vue';\nexport default {\n  name: 'ModalVerMantenciones',\n  components: {\n    SvgIcon\n  },\n  props: {\n    patente: {\n      type: String,\n      required: true\n    },\n    mantenciones: {\n      type: Array,\n      required: true\n    }\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      const f = new Date(fechaStr);\n      return f.toLocaleString();\n    }\n  },\n  computed: {\n    mantencionesConEstado() {\n      return this.mantenciones.map(m => {\n        let umbral;\n        let faltante;\n        let estado;\n        if (m.tipo === 'Por Odómetro') {\n          umbral = Number(m.ultimo) + Number(m.intervalo);\n          faltante = umbral - Number(m.actual);\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= Number(m.alerta)) estado = 'Próxima mantención';else estado = 'OK';\n        } else {\n          const [yyyy, mm, dd] = m.ultimo.split('-');\n          const ultimaFecha = new Date(yyyy, mm - 1, dd);\n          const intervaloDias = Number(m.intervalo) || 0;\n          const umbralDate = new Date(ultimaFecha.getFullYear(), ultimaFecha.getMonth(), ultimaFecha.getDate() + intervaloDias);\n          faltante = Math.ceil((umbralDate - new Date()) / (1000 * 60 * 60 * 24));\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= Number(m.alerta)) estado = 'Próxima mantención';else estado = 'OK';\n        }\n        return {\n          ...m,\n          umbral,\n          faltante,\n          estado\n        };\n      });\n    }\n  }\n};","map":{"version":3,"names":["SvgIcon","name","components","props","patente","type","String","required","mantenciones","Array","methods","formatFecha","fechaStr","f","Date","toLocaleString","computed","mantencionesConEstado","map","m","umbral","faltante","estado","tipo","Number","ultimo","intervalo"