ded-lg shadow p-3 sm:p-4 flex flex-col gap-2 border border-gray-300 dark:border-gray-700 text-sm\">\n\n            <!-- Nombre y tipo -->\n            <div class=\"flex justify-between items-center mb-1\">\n              <span class=\"font-semibold text-sm sm:text-base truncate\">{{ m.nombre || m.name }}</span>\n              <span class=\"text-xs px-2 py-0.5 rounded bg-blue-600 text-white\">{{ m.tipo || m.type }}</span>\n            </div>\n\n            <!-- Datos principales -->\n            <div class=\"grid grid-cols-2 gap-2\">\n              <div>\n                <span class=\"text-gray-500 text-xs\">Intervalo</span>\n                <div class=\"font-medium\">{{ m.intervalo }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Último</span>\n                <div class=\"font-medium\">{{ m.ultimo }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Umbral</span>\n                <div class=\"font-medium\">{{ m.umbral }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Actual</span>\n                <div class=\"font-medium\">{{ m.actual }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Faltante</span>\n                <div class=\"font-medium\">{{ m.faltante }}</div>\n              </div>\n              <div>\n                <span class=\"text-gray-500 text-xs\">Alerta</span>\n                <div class=\"font-medium\">{{ m.alerta || 0 }}</div>\n              </div>\n              <div class=\"col-span-2\">\n                <span class=\"text-gray-500 text-xs\">Email</span>\n                <div class=\"font-medium truncate\">{{ m.email || m.correo || 'N/A' }}</div>\n              </div>\n              <div class=\"col-span-2\">\n                <span class=\"text-gray-500 text-xs\">Creación</span>\n                <div class=\"font-medium\">{{ formatFecha(m.fechaCreacion) }}</div>\n              </div>\n            </div>\n\n            <!-- Estado e imágenes -->\n            <div class=\"flex items-center justify-between mt-2\">\n              <div>\n                <img\n                  v-if=\"m.estado === 'OK'\"\n                  src=\"https://sinergygroup.cl/Mantenciones/img/verde.png\"\n                  class=\"w-10 h-10 sm:w-12 sm:h-12 object-contain\" />\n                <img\n                  v-else-if=\"m.estado === 'Próxima mantención'\"\n                  src=\"https://sinergygroup.cl/Mantenciones/img/amarillo.png\"\n                  class=\"w-10 h-10 sm:w-12 sm:h-12 object-contain\" />\n                <img\n                  v-else-if=\"m.estado === 'Requiere mantención'\"\n                  src=\"https://sinergygroup.cl/Mantenciones/img/rojo.png\"\n                  class=\"w-10 h-10 sm:w-12 sm:h-12 object-contain\" />\n              </div>\n\n              <!-- Botones -->\n              <div class=\"flex gap-2 sm:gap-3\">\n                <button\n                  @click=\"$emit('editar', idx)\"\n                  class=\"bg-yellow-500 hover:bg-yellow-600 text-white px-2 py-1 rounded text-xs sm:text-sm\">Editar</button>\n                <button\n                  @click=\"$emit('eliminar', idx)\"\n                  class=\"bg-red-500 hover:bg-red-600 text-white px-2 py-1 rounded text-xs sm:text-sm\">Eliminar</button>\n              </div>\n            </div>\n          </div>\n\n          <!-- Si no hay mantenciones -->\n          <div\n            v-if=\"!mantenciones || mantenciones.length === 0\"\n            class=\"col-span-full text-center py-6 text-gray-500 dark:text-gray-400\">\n            No hay mantenciones registradas\n          </div>\n        </div>\n\n        <!-- Paginación -->\n        <div v-if=\"totalPaginas > 1\" class=\"flex items-center gap-2\">\n          <button\n            @click=\"paginaActual--\"\n            :disabled=\"paginaActual === 1\"\n            class=\"px-3 py-1 rounded bg-gray-200 dark:bg-gray-700 hover:bg-gray-300 dark:hover:bg-gray-600 disabled:opacity-50\">\n            Anterior\n          </button>\n          <span class=\"text-sm\">Página {{ paginaActual }} de {{ totalPaginas }}</span>\n          <button\n            @click=\"paginaActual++\"\n            :disabled=\"paginaActual === totalPaginas\"\n            class=\"px-3 py-1 rounded bg-gray-200 dark:bg-gray-700 hover:bg-gray-300 dark:hover:bg-gray-600 disabled:opacity-50\">\n            Siguiente\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport SvgIcon from '@/components/icons/SvgIcon.vue'\n\nexport default {\n  name: 'ModalVerMantenciones',\n  components: { SvgIcon },\n  props: {\n    patente: { type: String, required: true },\n    mantenciones: { type: Array, required: true },\n    horometrosActuales: { type: Object, default: () => ({}) }\n  },\n  data() {\n    return {\n      paginaActual: 1,\n      itemsPorPagina: 3\n    }\n  },\n  computed: {\n    mantencionesConEstado() {\n      return this.mantenciones.map((m) => {\n        const tipo = (m.tipo || m.type || '').toString().trim()\n        const intervalo = Number(m.intervalo || 0)\n        const alerta = Number(m.alerta || 0)\n\n        let umbral = null\n        let faltante = null\n        let estado = 'Desconocido'\n        let actual = 0\n        let horaRegistro = m.horaRegistro || null\n\n        if (tipo === 'Por Odómetro') {\n          const vehiculo = this.$parent.mantenciones.find(v => v.patente === this.patente)\n          actual = vehiculo ? Number(vehiculo.odometro) : Number(m.actual || 0)\n          const ultimo = Number(m.ultimo || 0)\n          const siguiente = ultimo + intervalo\n          umbral = siguiente\n          faltante = siguiente - actual\n          if (faltante <= 0) estado = 'Requiere mantención'\n          else if (faltante <= alerta) estado = 'Próxima mantención'\n          else estado = 'OK'\n        } else if (tipo === 'Por Horómetro') {\n          actual = this.horometrosActuales[this.patente] ?? Number(m.actual || 0)\n          const ultimo = Number(m.ultimo || 0)\n          const siguiente = ultimo + intervalo\n          umbral = siguiente\n          faltante = siguiente - actual\n          if (faltante <= 0) estado = 'Requiere mantención'\n          else if (faltante <= alerta) estado = 'Próxima mantención'\n          else estado = 'OK'\n        } else if (tipo === 'Por Tiempo') {\n          let ultimaFecha\n          if (typeof m.ultimo === 'string') {\n            const parts = m.ultimo.split('-')\n            if (parts.length === 3 && parts[0].length === 4) {\n              ultimaFecha = new Date(m.ultimo)\n            } else if (parts.length === 3) {\n              const [dd, mm, yyyy] = parts\n              ultimaFecha = new Date(Number(yyyy), Number(mm) - 1, Number(dd))\n            } else {\n              ultimaFecha = new Date(m.ultimo)\n            }\n          } else {\n            ultimaFecha = new Date()\n          }\n\n          if (!isNaN(ultimaFecha.getTime())) {\n            const umbralDate = new Date(ultimaFecha)\n            umbralDate.setDate(umbralDate.getDate() + intervalo)\n            const hoy = new Date()\n            faltante = Math.ceil(\n              (umbralDate - new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate())) /\n                (1000 * 60 * 60 * 24)\n            )\n            umbral = umbralDate.toISOString().split('T')[0]\n\n            if (faltante <= 0) estado = 'Requiere mantención'\n            else if (faltante <= alerta) estado = 'Próxima mantención'\n            else estado = 'OK'\n\n            actual = new Date().toISOString().split('T')[0]\n          }\n        }\n\n        return { ...m, umbral, faltante, estado, actual, horaRegistro }\n      })\n    },\n    mantencionesPaginadas() {\n      const inicio = (this.paginaActual - 1) * this.itemsPorPagina\n      const fin = inicio + this.itemsPorPagina\n      return this.mantencionesConEstado.slice(inicio, fin)\n    },\n    totalPaginas() {\n      return Math.ceil(this.mantencionesConEstado.length / this.itemsPorPagina)\n    }\n  },\n  watch: {\n    mantencionesConEstado() {\n      this.paginaActual = 1\n    }\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      const f = new Date(fechaStr)\n      return f.toLocaleString()\n    }\n  }\n}\n</script>\n"],"mappings":";;;AAgIA,OAAOA,OAAM,MAAO,gCAA+B;AAEnD,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAE;IAAEF;EAAQ,CAAC;EACvBG,KAAK,EAAE;IACLC,OAAO,EAAE;MAAEC,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACzCC,YAAY,EAAE;MAAEH,IAAI,EAAEI,KAAK;MAAEF,QAAQ,EAAE;IAAK,CAAC;IAC7CG,kBAAkB,EAAE;MAAEL,IAAI,EAAEM,MAAM;MAAEC,OAAO,EAAEA,CAAA,MAAO,CAAC,CAAC;IAAE;EAC1D,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;MACfC,cAAc,EAAE;IAClB;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,qBAAqBA,CAAA,EAAG;MACtB,OAAO,IAAI,CAACT,YAAY,CAACU,GAAG,CAAEC,CAAC,IAAK;QAClC,MAAMC,IAAG,GAAI,CAACD,CAAC,CAACC,IAAG,IAAKD,CAAC,CAACd,IAAG,IAAK,EAAE,EAAEgB,QAAQ,CAAC,CAAC,CAACC,IAAI,CAAC;QACtD,MAAMC,SAAQ,GAAIC,MAAM,CAACL,CAAC,CAACI,SAAQ,IAAK,CAAC;QACzC,MAAME,MAAK,GAAID,MAAM,CAACL,CAAC,CAACM,MAAK,IAAK,CAAC;QAEnC,IAAIC,MAAK,GAAI,IAAG;QAChB,IAAIC,QAAO,GAAI,IAAG;QAClB,IAAIC,MAAK,GAAI,aAAY;QACzB,IAAIC,MAAK,GAAI;QACb,IAAIC,YAAW,GAAIX,CAAC,CAACW,YAAW,IAAK,IAAG;QAExC,IAAIV,IAAG,KAAM,cAAc,EAAE;UAC3B,MAAMW,QAAO,GAAI,IAAI,CAACC,OAAO,CAACxB,YAAY,CAACyB,IAAI,CAACC,CAAA,IAAKA,CAAC,CAAC9B,OAAM,KAAM,IAAI,CAACA,OAAO;UAC/EyB,MAAK,GAAIE,QAAO,GAAIP,MAAM,CAACO,QAAQ,CAACI,QAAQ,IAAIX,MAAM,CAACL,CAAC,CAACU,MAAK,IAAK,CAAC;UACpE,MAAMO,MAAK,GAAIZ,MAAM,CAACL,CAAC,CAACiB,MAAK,IAAK,CAAC;UACnC,MAAMC,SAAQ,GAAID,MAAK,GAAIb,SAAQ;UACnCG,MAAK,GAAIW,SAAQ;UACjBV,QAAO,GAAIU,SAAQ,GAAIR,MAAK;UAC5B,IAAIF,QAAO,IAAK,CAAC,EAAEC,MAAK,GAAI,qBAAoB,MAC3C,IAAID,QAAO,IAAKF,MAAM,EAAEG,MAAK,GAAI,oBAAmB,MACpDA,MAAK,GAAI,IAAG;QACnB,OAAO,IAAIR,IAAG,KAAM,eAAe,EAAE;UACnCS,MAAK,GAAI,IAAI,CAACnB,kBAAkB,CAAC,IAAI,CAACN,OAAO,KAAKoB,MAAM,CAACL,CAAC,CAACU,MAAK,IAAK,CAAC;UACtE,MAA