pany.contact\" placeholder=\"Contacto\" required />\n        <input v-model=\"company.city\" placeholder=\"Ciudad\" required />\n      </form>\n\n      <div v-else class=\"grid grid-cols-2 gap-2\">\n        <div v-for=\"t in templates\" :key=\"t.id\" @click=\"company.templateId = t.id\" :class=\"company.templateId === t.id ? 'border-orange-500' : 'border-gray-300'\">\n          <h3>{{ t.name }}</h3>\n          <p>{{ t.description }}</p>\n        </div>\n      </div>\n\n      <div class=\"flex justify-between mt-4\">\n        <button @click=\"cancel\">Cancelar</button>\n        <button v-if=\"step===1\" @click=\"nextStep\">Siguiente</button>\n        <button v-else @click=\"add\" :disabled=\"!company.templateId\">Crear Empresa</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref } from 'vue'\nconst props = defineProps({ show: Boolean, templates: Array })\nconst emit = defineEmits(['update:show', 'add'])\n\nconst company = ref({ name: '', rut: '', contact: '', city: '', templateId: null })\nconst step = ref(1)\n\nconst nextStep = () => step.value = 2\nconst cancel = () => { step.value = 1; company.value = { name: '', rut: '', contact: '', city: '', templateId: null }; emit('update:show', false) }\nconst add = () => { emit('add', { ...company.value }); cancel() }\n</script>\n"],"mappings":"AA6BA,SAASA,GAAG,QAAQ,KAAI;;;;;;;;;;;;;IACxB,MAAMC,KAAK,GAAGC,OAAA;IACd,MAAMC,IAAI,GAAGC,MAAA;IAEb,MAAMC,OAAO,GAAGL,GAAG,CAAC;MAAEM,IAAI,EAAE,EAAE;MAAEC,GAAG,EAAE,EAAE;MAAEC,OAAO,EAAE,EAAE;MAAEC,IAAI,EAAE,EAAE;MAAEC,UAAU,EAAE;IAAK,CAAC;IAClF,MAAMC,IAAI,GAAGX,GAAG,CAAC,CAAC;IAElB,MAAMY,QAAQ,GAAGA,CAAA,KAAMD,IAAI,CAACE,KAAK,GAAG;IACpC,MAAMC,MAAM,GAAGA,CAAA,KAAM;MAAEH,IAAI,CAACE,KAAK,GAAG,CAAC;MAAER,OAAO,CAACQ,KAAK,GAAG;QAAEP,IAAI,EAAE,EAAE;QAAEC,GAAG,EAAE,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,IAAI,EAAE,EAAE;QAAEC,UAAU,EAAE;MAAK,CAAC;MAAEP,IAAI,CAAC,aAAa,EAAE,KAAK,CAAC;IAAC;IAClJ,MAAMY,GAAG,GAAGA,CAAA,KAAM;MAAEZ,IAAI,CAAC,KAAK,EAAE;QAAE,GAAGE,OAAO,CAACQ;MA