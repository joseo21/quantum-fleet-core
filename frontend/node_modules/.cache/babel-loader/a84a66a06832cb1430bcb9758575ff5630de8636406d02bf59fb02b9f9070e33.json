presa, se llenan los campos\nwatch(\n  () => props.empresa,\n  (newVal) => {\n    if (newVal) {\n      form.value = { ...newVal }\n    }\n  },\n  { immediate: true }\n)\n\nconst submit = () => {\n  emit('update', { ...form.value })\n  close()\n}\n\nconst close = () => {\n  emit('close')\n}\n</script>\n\n<style scoped>\n.input {\n  @apply w-full border border-gray-300 dark:border-gray-700 rounded-lg px-3 py-2 bg-white dark:bg-gray-800 focus:outline-none focus:ring-2 focus:ring-blue-500;\n}\n</style>\n"],"mappings":"AAmDA,SAASA,GAAG,EAAEC,KAAK,QAAQ,KAAI;;;;;;;;;;;;;IAE/B,MAAMC,KAAK,GAAGC,OAAA;IAKd,MAAMC,IAAI,GAAGC,MAAA;IAEb,MAAMC,IAAI,GAAGN,GAAG,CAAC;MACfO,IAAI,EAAE,EAAE;MACRC,GAAG,EAAE,EAAE;MACPC,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE;IAChB,CAAC;;IAED;IACAX,KAAK,CACH,MAAMC,KAAK,CAACW,OAAO,EAClBC,MAAM,IAAK;MACV,IAAIA,MAAM,EAAE;QACVR,IAAI,CAACS,KAAK,GAAG;UAAE,GAAGD;QAAO;MAC3B;IACF,CAAC,EACD;MAAEE,SAAS,EAAE;IAAK,CACpB;IAEA,MAAMC,MAAM,GAAGA,CAAA,KAAM;MACnBb,IAAI,CAAC,QAAQ,EAAE;QAAE,GAAGE,IAAI,CAACS;MAAM,CAAC;MAChCG,KAAK,CAAC;IACR;IAEA,MAAMA,KAAK,GAAGA,CAAA,KAAM;MAClBd,IAAI,CAAC,OAAO;IACd","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}