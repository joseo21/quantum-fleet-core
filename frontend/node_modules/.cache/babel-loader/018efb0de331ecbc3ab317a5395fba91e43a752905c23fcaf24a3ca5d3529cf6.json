{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nexport default {\n  name: 'ModalVerMantenciones',\n  data() {\n    return {\n      // Solo para pruebas\n      mantenciones: [{\n        nombre: 'Cambio Aceite',\n        tipo: 'Por Odómetro',\n        ultimo: 5000,\n        intervalo: 10000,\n        actual: 12000\n      }, {\n        nombre: 'Revisión Técnica',\n        tipo: 'Por Tiempo',\n        ultimo: '2025-10-23',\n        intervalo: 365,\n        actual: new Date().toISOString().split('T')[0]\n      }, {\n        nombre: 'Filtro Aire',\n        tipo: 'Por Odómetro',\n        ultimo: 2000,\n        intervalo: 500,\n        actual: 2500\n      }],\n      patente: 'ABC-123' // solo para prueba\n    };\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      const f = new Date(fechaStr);\n      return f.toLocaleString();\n    }\n  },\n  mounted() {\n    const hoy = new Date();\n    const pasada = new Date(hoy);\n    pasada.setDate(hoy.getDate() - 1);\n    const futura = new Date(hoy);\n    futura.setDate(hoy.getDate() + 1);\n    this.mantenciones.push({\n      nombre: 'Test OK',\n      tipo: 'Por Tiempo',\n      ultimo: hoy.toISOString().split('T')[0],\n      intervalo: 365,\n      actual: hoy.toISOString().split('T')[0],\n      alerta: 10\n    }, {\n      nombre: 'Test Próxima',\n      tipo: 'Por Tiempo',\n      ultimo: pasada.toISOString().split('T')[0],\n      intervalo: 2,\n      actual: hoy.toISOString().split('T')[0],\n      alerta: 5\n    }, {\n      nombre: 'Test Requiere',\n      tipo: 'Por Tiempo',\n      ultimo: pasada.toISOString().split('T')[0],\n      intervalo: 1,\n      actual: hoy.toISOString().split('T')[0],\n      alerta: 5\n    });\n  },\n  computed: {\n    mantencionesConEstado() {\n      return this.mantenciones.map(m => {\n        let umbral;\n        let faltante;\n        let estado;\n        if (m.tipo === 'Por Odómetro') {\n          umbral = Number(m.ultimo) + Number(m.intervalo);\n          faltante = umbral - Number(m.actual);\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= Number(m.alerta)) estado = 'Próxima mantención';else estado = 'OK';\n        } else {\n          const [yyyy, mm, dd] = m.ultimo.split('-');\n          const ultimaFecha = new Date(yyyy, mm - 1, dd);\n          const intervaloDias = Number(m.intervalo) || 0;\n          const umbralDate = new Date(ultimaFecha.getFullYear(), ultimaFecha.getMonth(), ultimaFecha.getDate() + intervaloDias);\n          faltante = Math.ceil((umbralDate - new Date()) / (1000 * 60 * 60 * 24));\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= Number(m.alerta)) estado = 'Próxima mantención';else estado = 'OK';\n        }\n        return {\n          ...m,\n          umbral,\n          faltante,\n          estado\n        };\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","mantenciones","nombre","tipo","ultimo","intervalo","actual","Date","toISOString","split","patente","methods","formatFecha","fechaStr","f","toLocaleString","mounted","hoy","pasada","setDate","getDate","futura","push","alerta","computed","mantencionesConEstado","map","m","umbral","faltante","estado","Number","yyyy","mm","dd","ultimaFecha","intervaloDias","umbralDate","getFullYear","getMonth","Math","ceil"],"sources":["/home/sebastian/quantum-fleet-core/frontend/src/components/MantencionesUi/ModalVerMantenciones.vue"],"sourcesContent":["<template>\n    <div class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-4\">\n        <div\n            class=\"bg-white dark:bg-gray-900 text-gray-900 dark:text-gray-100 rounded-lg relative w-full max-w-5xl h-auto p-6 border border-gray-300 dark:border-gray-700 shadow-xl overflow-auto\">\n\n            <!-- Header -->\n            <h2 class=\"text-2xl font-bold mb-4 text-center text-[#102372] dark:text-[#ff6600]\">\n                Mantenciones Vehículo {{ patente }}\n            </h2>\n\n            <button @click=\"$emit('close')\"\n                class=\"absolute top-4 right-4 p-2 rounded hover:bg-gray-200 dark:hover:bg-gray-700 transition\"\n                title=\"Cerrar\">\n                ✖\n            </button>\n\n            <!-- Tabla Mantenciones -->\n            <div class=\"overflow-x-auto\">\n                <table class=\"min-w-full text-xs sm:text-sm md:text-base text-center border-collapse\">\n                    <thead class=\"bg-[#102372] dark:bg-[#102372] sticky top-0 z-10 text-gray-100\">\n                        <tr>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Nombre</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Tipo</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Intervalo</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Último</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Umbral</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Actual</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Faltante</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Fecha Creación</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Estado</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Editar</th>\n                            <th class=\"py-2 px-3 border-b border-gray-300 dark:border-gray-600\">Eliminar</th>\n                        </tr>\n                    </thead>\n\n                    <tbody>\n                        <tr v-for=\"(m, idx) in mantencionesConEstado\" :key=\"idx\"\n                            class=\"hover:bg-gray-50 dark:hover:bg-gray-700 border-b dark:border-gray-600\">\n\n\n                            <td class=\"py-2 px-3\">{{ m.nombre || m.name }}</td>\n                            <td class=\"py-2 px-3\">{{ m.tipo || m.type }}</td>\n                            <td class=\"py-2 px-3\">{{ m.intervalo }}</td>\n                            <td class=\"py-2 px-3\">{{ m.ultimo }}</td>\n                            <td class=\"py-2 px-3\">{{ m.umbral }}</td>\n                            <td class=\"py-2 px-3\">{{ m.actual }}</td>\n                            <td class=\"py-2 px-3\">{{ m.faltante }}</td>\n                            <td class=\"py-2 px-3\">{{ formatFecha(m.fechaCreacion) }}</td>\n                            <td class=\"py-2 px-3\">\n                                <div class=\"flex items-center justify-center\">\n                                    <img v-if=\"m.estado === 'OK'\"\n                                        src=\"https://sinergygroup.cl/Mantenciones/img/verde.png\"\n                                        class=\"w-16 h-16 object-contain\" />\n                                    <img v-else-if=\"m.estado === 'Próxima mantención'\"\n                                        src=\"https://sinergygroup.cl/Mantenciones/img/amarillo.png\"\n                                        class=\"w-16 h-16 object-contain\" />\n                                    <img v-else-if=\"m.estado === 'Requiere mantención'\"\n                                        src=\"https://sinergygroup.cl/Mantenciones/img/rojo.png\"\n                                        class=\"w-16 h-16 object-contain\" />\n                                </div>\n                            </td>\n\n\n\n                            <td class=\"py-2 px-3\">\n                                <button @click=\"$emit('editar', idx)\"\n                                    class=\"bg-yellow-500 hover:bg-yellow-600 text-white px-2 py-1 rounded text-xs\">\n                                    Editar\n                                </button>\n                            </td>\n\n                            <td class=\"py-2 px-3\">\n                                <button @click=\"$emit('eliminar', idx)\"\n                                    class=\"bg-red-500 hover:bg-red-600 text-white px-2 py-1 rounded text-xs\">\n                                    Eliminar\n                                </button>\n                            </td>\n\n                        </tr>\n\n                        <tr v-if=\"!mantenciones || mantenciones.length === 0\">\n                            <td colspan=\"11\" class=\"text-center py-4 text-gray-500 dark:text-gray-400\">\n                                No hay mantenciones registradas\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'ModalVerMantenciones',\n    data() {\n        return {\n            // Solo para pruebas\n            mantenciones: [\n                { nombre: 'Cambio Aceite', tipo: 'Por Odómetro', ultimo: 5000, intervalo: 10000, actual: 12000 },\n                { nombre: 'Revisión Técnica', tipo: 'Por Tiempo', ultimo: '2025-10-23', intervalo: 365, actual: new Date().toISOString().split('T')[0] },\n                { nombre: 'Filtro Aire', tipo: 'Por Odómetro', ultimo: 2000, intervalo: 500, actual: 2500 }\n            ],\n            patente: 'ABC-123' // solo para prueba\n        }\n    },\n\n\n    methods: {\n        formatFecha(fechaStr) {\n            const f = new Date(fechaStr)\n            return f.toLocaleString()\n        }\n    },\n    mounted() {\n        const hoy = new Date();\n        const pasada = new Date(hoy);\n        pasada.setDate(hoy.getDate() - 1);\n        const futura = new Date(hoy);\n        futura.setDate(hoy.getDate() + 1);\n\n        this.mantenciones.push(\n            { nombre: 'Test OK', tipo: 'Por Tiempo', ultimo: hoy.toISOString().split('T')[0], intervalo: 365, actual: hoy.toISOString().split('T')[0], alerta: 10 },\n            { nombre: 'Test Próxima', tipo: 'Por Tiempo', ultimo: pasada.toISOString().split('T')[0], intervalo: 2, actual: hoy.toISOString().split('T')[0], alerta: 5 },\n            { nombre: 'Test Requiere', tipo: 'Por Tiempo', ultimo: pasada.toISOString().split('T')[0], intervalo: 1, actual: hoy.toISOString().split('T')[0], alerta: 5 }\n        );\n\n    },\n    computed: {\n        mantencionesConEstado() {\n            return this.mantenciones.map(m => {\n                let umbral;\n                let faltante;\n                let estado;\n\n                if (m.tipo === 'Por Odómetro') {\n                    umbral = Number(m.ultimo) + Number(m.intervalo);\n                    faltante = umbral - Number(m.actual);\n\n                    if (faltante <= 0) estado = 'Requiere mantención';\n                    else if (faltante <= Number(m.alerta)) estado = 'Próxima mantención';\n                    else estado = 'OK';\n                } else {\n                    const [yyyy, mm, dd] = m.ultimo.split('-');\n                    const ultimaFecha = new Date(yyyy, mm - 1, dd);\n                    const intervaloDias = Number(m.intervalo) || 0;\n                    const umbralDate = new Date(\n                        ultimaFecha.getFullYear(),\n                        ultimaFecha.getMonth(),\n                        ultimaFecha.getDate() + intervaloDias\n                    );\n\n                    faltante = Math.ceil((umbralDate - new Date()) / (1000 * 60 * 60 * 24));\n\n                    if (faltante <= 0) estado = 'Requiere mantención';\n                    else if (faltante <= Number(m.alerta)) estado = 'Próxima mantención';\n                    else estado = 'OK';\n                }\n\n                return { ...m, umbral, faltante, estado };\n            });\n        }\n\n    }\n\n}\n</script>\n\n<style scoped>\nbutton {\n    transition: all 0.2s ease-in-out;\n}\n</style>\n"],"mappings":";;;AA6FA,eAAe;EACXA,IAAI,EAAE,sBAAsB;EAC5BC,IAAIA,CAAA,EAAG;IACH,OAAO;MACH;MACAC,YAAY,EAAE,CACV;QAAEC,MAAM,EAAE,eAAe;QAAEC,IAAI,EAAE,cAAc;QAAEC,MAAM,EAAE,IAAI;QAAEC,SAAS,EAAE,KAAK;QAAEC,MAAM,EAAE;MAAM,CAAC,EAChG;QAAEJ,MAAM,EAAE,kBAAkB;QAAEC,IAAI,EAAE,YAAY;QAAEC,MAAM,EAAE,YAAY;QAAEC,SAAS,EAAE,GAAG;QAAEC,MAAM,EAAE,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC;MAAE,CAAC,EACxI;QAAEP,MAAM,EAAE,aAAa;QAAEC,IAAI,EAAE,cAAc;QAAEC,MAAM,EAAE,IAAI;QAAEC,SAAS,EAAE,GAAG;QAAEC,MAAM,EAAE;MAAK,EAC7F;MACDI,OAAO,EAAE,SAAQ,CAAE;IACvB;EACJ,CAAC;EAGDC,OAAO,EAAE;IACLC,WAAWA,CAACC,QAAQ,EAAE;MAClB,MAAMC,CAAA,GAAI,IAAIP,IAAI,CAACM,QAAQ;MAC3B,OAAOC,CAAC,CAACC,cAAc,CAAC;IAC5B;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,MAAMC,GAAE,GAAI,IAAIV,IAAI,CAAC,CAAC;IACtB,MAAMW,MAAK,GAAI,IAAIX,IAAI,CAACU,GAAG,CAAC;IAC5BC,MAAM,CAACC,OAAO,CAACF,GAAG,CAACG,OAAO,CAAC,IAAI,CAAC,CAAC;IACjC,MAAMC,MAAK,GAAI,IAAId,IAAI,CAACU,GAAG,CAAC;IAC5BI,MAAM,CAACF,OAAO,CAACF,GAAG,CAACG,OAAO,CAAC,IAAI,CAAC,CAAC;IAEjC,IAAI,CAACnB,YAAY,CAACqB,IAAI,CAClB;MAAEpB,MAAM,EAAE,SAAS;MAAEC,IAAI,EAAE,YAAY;MAAEC,MAAM,EAAEa,GAAG,CAACT,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MAAEJ,SAAS,EAAE,GAAG;MAAEC,MAAM,EAAEW,GAAG,CAACT,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MAAEc,MAAM,EAAE;IAAG,CAAC,EACvJ;MAAErB,MAAM,EAAE,cAAc;MAAEC,IAAI,EAAE,YAAY;MAAEC,MAAM,EAAEc,MAAM,CAACV,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MAAEJ,SAAS,EAAE,CAAC;MAAEC,MAAM,EAAEW,GAAG,CAACT,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MAAEc,MAAM,EAAE;IAAE,CAAC,EAC5J;MAAErB,MAAM,EAAE,eAAe;MAAEC,IAAI,EAAE,YAAY;MAAEC,MAAM,EAAEc,MAAM,CAACV,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MAAEJ,SAAS,EAAE,CAAC;MAAEC,MAAM,EAAEW,GAAG,CAACT,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MAAEc,MAAM,EAAE;IAAE,CAChK,CAAC;EAEL,CAAC;EACDC,QAAQ,EAAE;IACNC,qB