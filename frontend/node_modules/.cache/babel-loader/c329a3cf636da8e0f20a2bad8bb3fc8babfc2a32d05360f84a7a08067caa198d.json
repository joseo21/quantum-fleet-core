{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport SvgIcon from '@/components/icons/SvgIcon.vue';\nexport default {\n  name: 'ModalVerMantenciones',\n  components: {\n    SvgIcon\n  },\n  props: {\n    patente: {\n      type: String,\n      required: true\n    },\n    mantenciones: {\n      type: Array,\n      required: true\n    }\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      const f = new Date(fechaStr);\n      return f.toLocaleString();\n    }\n  },\n  computed: {\n    mantencionesConEstado() {\n      return this.mantenciones.map(m => {\n        const tipo = (m.tipo || m.type || '').toString().trim();\n        const intervalo = Number(m.intervalo || 0);\n        const alerta = Number(m.alerta || 0);\n        let umbral = null;\n        let faltante = null;\n        let estado = 'Desconocido';\n        if (tipo === 'Por Odómetro') {\n          const ultimo = Number(m.ultimo || 0);\n          const actual = Number(m.actual || 0);\n          const siguiente = ultimo + intervalo;\n          umbral = siguiente;\n          faltante = siguiente - actual;\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n        } else if (tipo === 'Por Tiempo') {\n          let ultimaFecha;\n          if (typeof m.ultimo === 'string' && m.ultimo.includes('-')) {\n            const parts = m.ultimo.split('-');\n            if (parts.length === 3 && parts[0].length === 4) ultimaFecha = new Date(m.ultimo);else if (parts.length === 3) {\n              const [dd, mm, yyyy] = parts;\n              ultimaFecha = new Date(Number(yyyy), Number(mm) - 1, Number(dd));\n            } else ultimaFecha = new Date(m.ultimo);\n          } else ultimaFecha = new Date();\n          if (isNaN(ultimaFecha.getTime())) {\n            estado = 'Desconocido';\n          } else {\n            const umbralDate = new Date(ultimaFecha);\n            umbralDate.setDate(umbralDate.getDate() + intervalo);\n            const hoy = new Date();\n            const diffMs = umbralDate - new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate());\n            faltante = Math.ceil(diffMs / (1000 * 60 * 60 * 24));\n            umbral = umbralDate.toISOString().split('T')[0];\n            if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n          }\n        }\n        return {\n          ...m,\n          umbral,\n          faltante,\n          estado\n        };\n      });\n    }\n  }\n};","map":{"version":3,"names":["SvgIcon","name","components","props","patente","type","String","required","mantenciones","Array","methods","formatFecha","fechaStr","f","Date","toLocaleString","computed","mantencionesConEstado","map","m","tipo","toString","trim","intervalo","Number","alerta","umbral","faltante","estado","ultimo","actual","siguiente","ultimaFecha","includes","parts","split","length","dd","mm","yyyy","isNaN","getTime","umbralDate","setDate","getDate","hoy","diffMs","getFullYear","getMonth","Math","ceil","toISOString"],"sources":["/home/sebastian/quantum-fleet-core/frontend/src/components/MantencionesUi/ModalVerMantenciones.vue"],"sourcesContent":["<template>\n  <div class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-2 sm:p-4\">\n    <div\n      class=\"bg-white dark:bg-gray-900 text-gray-900 dark:text-gray-100 rounded-lg relative w-full max-w-5xl h-auto p-4 sm:p-6 border border-gray-300 dark:border-gray-700 shadow-xl overflow-auto\">\n\n      <!-- Header -->\n      <h2 class=\"text-xl sm:text-2xl font-bold mb-4 text-center text-[#102372] dark:text-[#ff6600]\">\n        Mantenciones Vehículo {{ patente }}\n      </h2>\n\n      <button @click=\"$emit('close')\"\n        class=\"absolute top-4 right-4 p-2 rounded hover:bg-gray-200 dark:hover:bg-gray-700 transition\"\n        title=\"Cerrar\">\n        <SvgIcon name=\"close\" class=\"w-6 h-6\" />\n      </button>\n\n      <!-- Tabla Mantenciones -->\n      <div class=\"overflow-x-auto\">\n        <table class=\"min-w-full text-xs sm:text-sm md:text-base text-center border-collapse\">\n          <thead class=\"bg-[#102372] dark:bg-[#102372] sticky top-0 z-10 text-gray-100\">\n            <tr>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600\">Nombre</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600\">Tipo</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600 hidden sm:table-cell\">Intervalo</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600\">Último</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600 hidden sm:table-cell\">Umbral</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600 hidden sm:table-cell\">Actual</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600\">Faltante</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600 hidden sm:table-cell\">Fecha Creación</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600\">Estado</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600\">Editar</th>\n              <th class=\"py-2 px-2 sm:px-3 border-b border-gray-300 dark:border-gray-600\">Eliminar</th>\n            </tr>\n          </thead>\n\n          <tbody>\n            <tr v-for=\"(m, idx) in mantencionesConEstado\" :key=\"idx\"\n              class=\"hover:bg-gray-50 dark:hover:bg-gray-700 border-b dark:border-gray-600\">\n\n              <td class=\"py-1 px-2 sm:px-3\">{{ m.nombre || m.name }}</td>\n              <td class=\"py-1 px-2 sm:px-3\">{{ m.tipo || m.type }}</td>\n              <td class=\"py-1 px-2 sm:px-3 hidden sm:table-cell\">{{ m.intervalo }}</td>\n              <td class=\"py-1 px-2 sm:px-3\">{{ m.ultimo }}</td>\n              <td class=\"py-1 px-2 sm:px-3 hidden sm:table-cell\">{{ m.umbral }}</td>\n              <td class=\"py-1 px-2 sm:px-3 hidden sm:table-cell\">{{ m.actual }}</td>\n              <td class=\"py-1 px-2 sm:px-3\">{{ m.faltante }}</td>\n              <td class=\"py-1 px-2 sm:px-3 hidden sm:table-cell\">{{ formatFecha(m.fechaCreacion) }}</td>\n              <td class=\"py-1 px-2 sm:px-3\">\n                <div class=\"flex items-center justify-center\">\n                  <img v-if=\"m.estado === 'OK'\" src=\"https://sinergygroup.cl/Mantenciones/img/verde.png\"\n                    class=\"w-12 h-12 sm:w-16 sm:h-16 object-contain\" />\n                  <img v-else-if=\"m.estado === 'Próxima mantención'\" src=\"https://sinergygroup.cl/Mantenciones/img/amarillo.png\"\n                    class=\"w-12 h-12 sm:w-16 sm:h-16 object-contain\" />\n                  <img v-else-if=\"m.estado === 'Requiere mantención'\" src=\"https://sinergygroup.cl/Mantenciones/img/rojo.png\"\n                    class=\"w-12 h-12 sm:w-16 sm:h-16 object-contain\" />\n                </div>\n              </td>\n\n              <td class=\"py-1 px-2 sm:px-3\">\n                <button @click=\"$emit('editar', idx)\"\n                  class=\"bg-yellow-500 hover:bg-yellow-600 text-white px-2 py-1 rounded text-xs sm:text-sm w-full\">\n                  Editar\n                </button>\n              </td>\n\n              <td class=\"py-1 px-2 sm:px-3\">\n                <button @click=\"$emit('eliminar', idx)\"\n                  class=\"bg-red-500 hover:bg-red-600 text-white px-2 py-1 rounded text-xs sm:text-sm w-full\">\n                  Eliminar\n                </button>\n              </td>\n\n            </tr>\n\n            <tr v-if=\"!mantenciones || mantenciones.length === 0\">\n              <td colspan=\"11\" class=\"text-center py-4 text-gray-500 dark:text-gray-400\">\n                No hay mantenciones registradas\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport SvgIcon from '@/components/icons/SvgIcon.vue'\nexport default {\n  name: 'ModalVerMantenciones',\n  components: { SvgIcon },\n  props: {\n    patente: { type: String, required: true },\n    mantenciones: { type: Array, required: true }\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      const f = new Date(fechaStr)\n      return f.toLocaleString()\n    }\n  },\n  computed: {\n    mantencionesConEstado() {\n      return this.mantenciones.map((m) => {\n        const tipo = (m.tipo || m.type || '').toString().trim()\n        const intervalo = Number(m.intervalo || 0)\n        const alerta = Number(m.alerta || 0)\n\n        let umbral = null\n        let faltante = null\n        let estado = 'Desconocido'\n\n        if (tipo === 'Por Odómetro') {\n          const ultimo = Number(m.ultimo || 0)\n          const actual = Number(m.actual || 0)\n          const siguiente = ultimo + intervalo\n          umbral = siguiente\n          faltante = siguiente - actual\n          if (faltante <= 0) estado = 'Requiere mantención'\n          else if (faltante <= alerta) estado = 'Próxima mantención'\n          else estado = 'OK'\n        } else if (tipo === 'Por Tiempo') {\n          let ultimaFecha\n          if (typeof m.ultimo === 'string' && m.ultimo.includes('-')) {\n            const parts = m.ultimo.split('-')\n            if (parts.length === 3 && parts[0].length === 4) ultimaFecha = new Date(m.ultimo)\n            else if (parts.length === 3) {\n              const [dd, mm, yyyy] = parts\n              ultimaFecha = new Date(Number(yyyy), Number(mm) - 1, Number(dd))\n            } else ultimaFecha = new Date(m.ultimo)\n          } else ultimaFecha = new Date()\n\n          if (isNaN(ultimaFecha.getTime())) {\n            estado = 'Desconocido'\n          } else {\n            const umbralDate = new Date(ultimaFecha)\n            umbralDate.setDate(umbralDate.getDate() + intervalo)\n            const hoy = new Date()\n            const diffMs = umbralDate - new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate())\n            faltante = Math.ceil(diffMs / (1000 * 60 * 60 * 24))\n            umbral = umbralDate.toISOString().split('T')[0]\n            if (faltante <= 0) estado = 'Requiere mantención'\n            else if (faltante <= alerta) estado = 'Próxima mantención'\n            else estado = 'OK'\n          }\n        }\n\n        return { ...m, umbral, faltante, estado }\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\nbutton {\n  transition: all 0.2s ease-in-out;\n}\n</style>\n"],"mappings":";;AAwFA,OAAOA,OAAM,MAAO,gCAA+B;AACnD,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAE;IAAEF;EAAQ,CAAC;EACvBG,KAAK,EAAE;IACLC,OAAO,EAAE;MAAEC,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACzCC,YAAY,EAAE;MAAEH,IAAI,EAAEI,KAAK;MAAEF,QAAQ,EAAE;IAAK;EAC9C,CAAC;EACDG,OAAO,EAAE;IACPC,WAAWA,CAACC,QAAQ,EAAE;MACpB,MAAMC,CAAA,GAAI,IAAIC,IAAI,CAACF,QAAQ;MAC3B,OAAOC,CAAC,CAACE,cAAc,CAAC;IAC1B;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,qBAAqBA,CAAA,EAAG;MACtB,OAAO,IAAI,CAACT,YAAY,CAACU,GAAG,CAAEC,CAAC,IAAK;QAClC,MAAMC,IAAG,GAAI,CAACD,CAAC,CAACC,IAAG,IAAKD,CAAC,CAACd,IAAG,IAAK,EAAE,EAAEgB,QAAQ,CAAC,CAAC,CAACC,IAAI,CAAC;QACtD,MAAMC,SAAQ,GAAIC,MAAM,CAACL,CAAC,CAACI,SAAQ,IAAK,CAAC;QACzC,MAAME,MAAK,GAAID,MAAM,CAACL,CAAC,CAACM,MAAK,IAAK,CAAC;QAEnC,IAAIC,MAAK,GAAI,IAAG;QAChB,IAAIC,QAAO,GAAI,IAAG;QAClB,IAAIC,MAAK,GAAI,aAAY;QAEzB,IAAIR,IAAG,KAAM,cAAc,EAAE;UAC3B,MAAMS,MAAK,GAAIL,MAAM,CAACL,CAAC,CAACU,MAAK,IAAK,CAAC;UACnC,MAAMC,MAAK,GAAIN,MAAM,CAACL,CAAC,CAACW,MAAK,IAAK,CAAC;UACnC,MAAMC,SAAQ,GAAIF,MAAK,GAAIN,SAAQ;UACnCG,MAAK,GAAIK,SAAQ;UACjBJ,QAAO,GAAII,SAAQ,GAAID,MAAK;UAC5B,IAAIH,QAAO,IAAK,CAAC,EAAEC,MAAK,GAAI,qBAAoB,MAC3C,IAAID,QAAO,IAAKF,MAAM,EAAEG,MAAK,GAAI,oBAAmB,MACpDA,MAAK,GAAI,IAAG;QACnB,OAAO,IAAIR,IAAG,KAAM,YAAY,EAAE;UAChC,IAAIY,WAAU