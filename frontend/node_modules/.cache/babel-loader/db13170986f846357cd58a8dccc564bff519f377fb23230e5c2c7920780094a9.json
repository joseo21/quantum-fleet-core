 = 'Desconocido';\n            umbral = null;\n            faltante = null;\n          } else {\n            const umbralDate = new Date(ultimaFecha);\n            umbralDate.setDate(umbralDate.getDate() + intervalo);\n            const hoy = new Date();\n            const diffMs = umbralDate - new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate());\n            faltante = Math.ceil(diffMs / (1000 * 60 * 60 * 24));\n            umbral = umbralDate.toISOString().split('T')[0];\n            if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n          }\n        } else {\n          estado = 'Desconocido';\n        }\n        return {\n          ...m,\n          umbral,\n          faltante,\n          estado\n        };\n      });\n    }\n  }\n};","map":{"version":3,"names":["SvgIcon","name","components","props","patente","type","String","required","mantenciones","Array","methods","formatFecha","fechaStr","f","Date","toLocaleString","computed","mantencionesConEstado","map","m","tipo","toString","trim","intervalo","Number","alerta","umbral","faltante","estado","ultimo","actual","siguiente","ultimaFecha","includes","parts","split","length","dd","mm","yyyy","isNaN","getTime","umbralDate","setDate","getDate","hoy","diffMs","getFullYear","getMonth","Math","ceil","toISOString"],"sources":["/home/sebastian/quantum-fleet-core/frontend/src/components/MantencionesUi/ModalVerMantenciones.vue"],"sourcesContent":["<template>\n  <div class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-2 sm:p-4\">\n    <div\n      class=\"bg-white dark:bg-gray-900 text-gray-900 dark:text-gray-100 rounded-lg relative w-full max-w-5xl h-auto p-6 border border-gray-300 dark:border-gray-700 shadow-xl overflow-auto\">\n\n      <!-- Header -->\n      <h2 class=\"text-2xl font-bold mb-6 text-center text-[#102372] dark:text-[#ff6600]\">\n        Mantenciones Vehículo {{ patente }}\n      </h2>\n\n      <!-- Botón cerrar -->\n      <button @click=\"$emit('close')\"\n        class=\"absolute top-4 right-4 p-2 rounded hover:bg-gray-200 dark:hover:bg-gray-700 transition\"\n        title=\"Cerrar\">\n        <SvgIcon name=\"close\" class=\"w-6 h-6\" />\n      </button>\n\n      <!-- Cards -->\n      <div class=\"flex flex-col gap-6\">\n        <div v-for=\"(m, idx) in mantencionesConEstado\" :key=\"idx\"\n          class=\"bg-gray-100 dark:bg-gray-800 rounded-lg shadow p-5 flex flex-col gap-3 border border-gray-300 dark:border-gray-700\">\n\n          <!-- Nombre y tipo -->\n          <div class=\"flex justify-between items-center mb-2\">\n            <span class=\"font-semibold text-lg\">{{ m.nombre || m.name }}</span>\n            <span class=\"text-xs px-2 py-1 rounded bg-blue-600 text-white\">{{ m.tipo || m.type }}</span>\n          </div>\n\n          <!-- Datos principales -->\n          <div class=\"grid grid-cols-2 sm:grid-cols-3 gap-4\">\n            <div>\n              <span class=\"text-gray-500 text-xs\">Intervalo</span>\n              <div class=\"font-medium\">{{ m.intervalo }}</div>\n            </div>\n            <div>\n              <span class=\"text-gray-500 text-xs\">Último</span>\n              <div class=\"font-medium\">{{ m.ultimo }}</div>\n            </div>\n            <div>\n              <span class=\"text-gray-500 text-xs\">Umbral</span>\n              <div class=\"font-medium\">{{ m.umbral }}</div>\n            </div>\n            <div>\n              <span class=\"text-gray-500 text-xs\">Actual</span>\n              <div class=\"font-medium\">{{ m.actual }}</div>\n            </div>\n            <div>\n              <span class=\"text-gray-500 text-xs\">Faltante</span>\n              <div class=\"font-medium\">{{ m.faltante }}</div>\n            </div>\n            <div>\n              <span class=\"text-gray-500 text-xs\">Alerta</span>\n              <div class=\"font-medium\">{{ m.alerta || 0 }}</div>\n            </div>\n    