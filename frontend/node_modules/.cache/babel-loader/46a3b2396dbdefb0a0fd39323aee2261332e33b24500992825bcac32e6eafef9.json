      if (!searchTerm.value) return customers.value;\n      const term = searchTerm.value.toLowerCase();\n      return customers.value.filter(c => c.name.toLowerCase().includes(term) || c.rut.toLowerCase().includes(term) || c.contact.toLowerCase().includes(term) || c.createdAt.toLowerCase().includes(term));\n    });\n    const paginatedCustomers = computed(() => {\n      const start = (currentPage.value - 1) * rowsPerPage.value;\n      return filteredCustomers.value.slice(start, start + rowsPerPage.value);\n    });\n    const totalPages = computed(() => Math.ceil(filteredCustomers.value.length / rowsPerPage.value));\n    const toggleSelectAll = () => selectAll.value ? selectedIds.value = paginatedCustomers.value.map(c => c.id) : selectedIds.value = [];\n    const nextPage = () => {\n      if (currentPage.value < totalPages.value) currentPage.value++;\n    };\n    const previousPage = () => {\n      if (currentPage.value > 1) currentPage.value--;\n    };\n    const goToNextStep = () => step.value = 2;\n    const cancel = () => {\n      showAddModal.value = false;\n      step.value = 1;\n      selectedTemplate.value = null;\n      newCompany.value = {\n        name: '',\n        rut: '',\n        contact: '',\n        city: ''\n      };\n    };\n    const addCompany = () => {\n      if (!selectedTemplate.value) return;\n      const id = Date.now();\n      const createdAt = new Date().toISOString().slice(0, 10);\n      customers.value.push({\n        id,\n        createdAt,\n        ...newCompany.value,\n        templateId: selectedTemplate.value\n      });\n      cancel();\n    };\n    const deleteSelected = () => {\n      customers.value = customers.value.filter(c => !selectedIds.value.includes(c.id));\n      selectedIds.value = [];\n    };\n    const copyCustomerData = customer => {\n      const data = `Nombre: ${customer.name}\\nRUT: ${customer.rut}\\nContacto: ${customer.contact}`;\n      navigator.clipboard.writeText(data).then(() => {\n        copiedId.value = customer.id;\n        setTimeout(() => copiedId.value = null, 1000);\n      });\n    };\n    const editCustomer = customer => {\n      editingCompany.value = {\n        ...customer\n      };\n      if (!editingCompany.value.templateId) editingCompany.value.templateId = 1;\n      showEditSidebar.value = true;\n    };\n    const updateCompany = () => {\n      const index = customers.value.findIndex(c => c.id === editingCompany.value.id);\n      if (index !== -1) customers.value[index] = {\n        ...editingCompany.value\n      };\n      showEditSidebar.value = false;\n    };\n    watch(paginatedCustomers, () => selectAll.value = false);\n    const __returned__ = {\n      customers,\n      searchTerm,\n      selectedIds,\n      selectAll,\n      rowsPerPageOptions,\n      rowsPerPage,\n      currentPage,\n      showAddModal,\n      step,\n      selectedTemplate,\n      showEditSidebar,\n      editingCompany,\n      newCompany,\n      copiedId,\n      templates,\n      filteredCustomers,\n      paginatedCustomers,\n      totalPages,\n      toggleSelectAll,\n      nextPage,\n      previousPage,\n      goToNextStep,\n      cancel,\n      addCompany,\n      deleteSelected,\n      copyCustomerData,\n      editCustomer,\n      updateCompany,\n      ref,\n      computed,\n      watch,\n      SvgIcon\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","watch","SvgIcon","customers","id","createdAt","name","rut","contact","city","searchTerm","selectedIds","selectAll","rowsPerPageOptions","rowsPerPage","currentPage","showAddModal","step","selectedTemplate","showEditSidebar","editingCompany","templateId","newCompany","copiedId","templates","description","filteredCustomers","value","term","toLowerCase","filter","c","includes","paginatedCustomers","start","slice","totalPages","Math","ceil","length","toggleSelectAll","map","nextPage"