his.step = 1;\n        } else {\n          this.maintenance = {\n            name: '',\n            type: '',\n            date: '',\n            km: '',\n            details: '',\n            intervalo: '',\n            email: '',\n            alerta: '',\n            fechaUltima: ''\n          };\n          this.step = 1;\n        }\n      }\n    },\n    showAddModal(val) {\n      console.log('[ModalAgregar] watch showAddModal ->', val, 'selectedPatente:', this.selectedPatente);\n      if (val) {\n        this.step = 1;\n        this.selectedTemplate = null;\n\n        // Espera un ciclo del DOM para que las props (mantenciones) se sincronicen\n        nextTick(() => {\n          console.log('[ModalAgregar] prop mantenciones recibidas (tras nextTick):', this.mantenciones);\n          const nombresUsados = (this.mantenciones || []).map(m => {\n            return (m.nombre || m.name || '').toString().trim().toLowerCase();\n          });\n          console.log('[ModalAgregar] nombresUsados normalizados:', nombresUsados);\n          this.templates.forEach(t => {\n            const usado = nombresUsados.includes((t.name || '').toString().trim().toLowerCase());\n            t.used = usado;\n          });\n          console.log('[ModalAgregar] templates after marking used:', this.templates);\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["SvgIcon","nextTick","name","components","props","showAddModal","type","Boolean","required","selectedPatente","String","mantenciones","Array","selectedMantencion","Object","Number","default","kmActual","data","step","templates","id","description","used","selectedTemplate","maintenance","date","km","details","intervalo","email","alerta","fechaUltima","isModalButtonDisabled","methods","selectTemplate","template","console","log","goToNextStep","cancel","$emit","saveMaintenance","nuevaMantencion","nombre","tipo","ultimo","actual","Date","toISOString","split","fechaCreacion","index","findIndex","t","onlyNumber","e","char",