{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport SvgIcon from '@/components/icons/SvgIcon.vue';\nexport default {\n  name: 'ModalVerMantenciones',\n  components: {\n    SvgIcon\n  },\n  props: {\n    patente: {\n      type: String,\n      required: true\n    },\n    mantenciones: {\n      type: Array,\n      required: true\n    },\n    horometrosActuales: {\n      type: Object,\n      default: () => ({})\n    },\n    odometroActual: {\n      type: Number,\n      default: 0\n    }\n  },\n  methods: {\n    formatFecha(fechaStr) {\n      // Asegurarse de que la fecha es válida\n      if (!fechaStr) return 'N/A';\n      const f = new Date(fechaStr);\n      if (isNaN(f.getTime())) return fechaStr; // Retorna el string si no es una fecha válida\n\n      // Opciones de formato más claras y consistentes\n      const options = {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit'\n      };\n      return f.toLocaleString('es-CL', options); // Usa 'es-CL' o tu locale preferida\n    }\n  },\n  computed: {\n    mantencionesConEstado() {\n      return this.mantenciones.map(m => {\n        const tipo = (m.tipo || m.type || '').toString().trim();\n        const intervalo = Number(m.intervalo || 0);\n        const alerta = Number(m.alerta || 0);\n        let umbral = null;\n        let faltante = null;\n        let estado = 'Desconocido';\n        let actual = 0;\n        let horaRegistro = m.horaRegistro || null;\n\n        // Odómetro\n        if (tipo === 'Por Odómetro') {\n          actual = Number(this.odometroActual || m.actual || 0);\n          const ultimo = Number(m.ultimo || 0);\n          const siguiente = ultimo + intervalo;\n          umbral = siguiente;\n          faltante = siguiente - actual;\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n        }\n\n        // Horómetro\n        else if (tipo === 'Por Horómetro') {\n          // Se usa el horometroActuales del vehiculo (con la patente) o el valor 'actual' de la mantención si no está en el objeto\n          actual = this.horometrosActuales[this.patente] ?? Number(m.actual || 0);\n          const ultimo = Number(m.ultimo || 0);\n          const siguiente = ultimo + intervalo;\n          umbral = siguiente;\n          faltante = siguiente - actual;\n          if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n        }\n\n        // Por Tiempo\n        else if (tipo === 'Por Tiempo') {\n          let ultimaFecha;\n          // Lógica de parsing de fecha mejorada para ser más robusta\n          if (typeof m.ultimo === 'string' && m.ultimo.trim() !== '') {\n            const parts = m.ultimo.split('-');\n            if (parts.length === 3) {\n              if (parts[0].length === 4) {\n                // Formato YYYY-MM-DD\n                ultimaFecha = new Date(m.ultimo + 'T00:00:00');\n              } else {\n                // Asumir formato DD-MM-YYYY\n                const [dd, mm, yyyy] = parts;\n                ultimaFecha = new Date(`${yyyy}-${mm}-${dd}T00:00:00`);\n              }\n            } else {\n              // Intentar parsear el string directamente\n              ultimaFecha = new Date(m.ultimo);\n            }\n          } else {\n            // Si no hay fecha de último registro, se puede asignar la fecha de creación o no calcular\n            // Por simplicidad, si es inválida, se mantendrá el estado 'Desconocido' si el cálculo no procede.\n            ultimaFecha = new Date(0); // Fecha inválida para el cálculo\n          }\n          if (!isNaN(ultimaFecha.getTime()) && ultimaFecha.getTime() > 0) {\n            const umbralDate = new Date(ultimaFecha);\n            umbralDate.setDate(umbralDate.getDate() + intervalo);\n            const hoy = new Date();\n\n            // Normalizar hoy a medianoche para la comparación de días\n            const hoyNormalizado = new Date(hoy.getFullYear(), hoy.getMonth(), hoy.getDate());\n\n            // Calcular la diferencia en días\n            const diffTime = umbralDate.getTime() - hoyNormalizado.getTime();\n            faltante = Math.ceil(diffTime / (1000 * 60 * 60 * 24));\n\n            // Formatear umbral para mostrar\n            umbral = umbralDate.toISOString().split('T')[0];\n            if (faltante <= 0) estado = 'Requiere mantención';else if (faltante <= alerta) estado = 'Próxima mantención';else estado = 'OK';\n            actual = new Date().toISOString().split('T')[0];\n          } else {\n            // Mantener los valores como null o 0 si la fecha inicial es inválida\n            umbral = 'N/A';\n            faltante = 'N/A';\n            actual = 'N/A';\n          }\n        }\n        return {\n          ...m,\n          umbral,\n          faltante,\n          estado,\n          actual,\n          horaRegistro\n        };\n      });\n    }\n  }\n};","map":{"version":3,"names":["SvgIcon","name","components","props","patente","type","String","required","mantenciones","Array","horometrosActuales","Object","default","odometroActual","Number","methods","formatFecha","fechaStr","f","Date","isNaN","getTime","options","year","month","day","hour","minute","toLocaleString","computed","mantencionesConEstado","map","m","tipo","toString","trim","intervalo","alerta","umbral","faltante","estado","actual","horaRegistro","ultimo","siguiente","ultimaFecha","parts","split","length","dd","mm","yyyy","umbralDate","setDate","getDate","hoy","hoyNormalizado","getFullYear","getMonth","diffTime","Math","ceil","toISOString"],"sources":["/home/sebastian/quantum-fleet-core/frontend/src/components/MantencionesUi/ModalVerMantenciones.vue"],"sourcesContent":["<template>\n    <div class=\"fixed inset-0 bg-black bg-opacity-70 flex items-center justify-center z-50 p