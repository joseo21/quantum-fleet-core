ttonDisabled: false\n    };\n  },\n  methods: {\n    selectTemplate(template) {\n      console.log('[ModalAgregar] selectTemplate:', template);\n      if (template.used) {\n        console.log('[ModalAgregar] selectTemplate -> template is used, ignoring');\n        return;\n      }\n      this.selectedTemplate = template;\n      this.maintenance.name = template.name;\n      this.maintenance.type = template.type;\n    },\n    goToNextStep() {\n      if (this.selectedTemplate) this.step = 2;\n    },\n    cancel() {\n      console.log('[ModalAgregar] cancel ‚Äî emitting close');\n      this.$emit('close');\n      this.step = 1;\n      this.selectedTemplate = null;\n      this.maintenance = {\n        name: '',\n        type: '',\n        date: '',\n        km: '',\n        details: '',\n        intervalo: '',\n        email: '',\n        alerta: '',\n        fechaUltima: ''\n      };\n    },\n    saveMaintenance() {\n      console.log('[ModalAgregar] saveMaintenance ‚Äî current maintenance:', this.maintenance);\n      if (!this.maintenance) return;\n      const nuevaMantencion = {\n        nombre: this.maintenance.name,\n        tipo: this.maintenance.type,\n        intervalo: Number(this.maintenance.intervalo || 0),\n        ultimo: this.selectedTemplate?.type === 'Por Od√≥metro' ? Number(this.maintenance.km || 0) : this.maintenance.fechaUltima,\n        actual: this.selectedTemplate?.type === 'Por Od√≥metro' ? Number(this.maintenance.km || 0) : new Date().toISOString().split('T')[0],\n        alerta: Number(this.maintenance.alerta || 0),\n        email: this.maintenance.email || '',\n        fechaCreacion: new Date().toISOString()\n      };\n      console.log('[ModalAgregar] Emitting save with:', nuevaMantencion);\n      this.$emit('save', nuevaMantencion);\n\n      // üîí Marcar este template como usado inmediatamente (esto solo afecta la instancia actual)\n      if (this.selectedTemplate) {\n        const index = this.templates.findIndex(t => t.id === this.selecte