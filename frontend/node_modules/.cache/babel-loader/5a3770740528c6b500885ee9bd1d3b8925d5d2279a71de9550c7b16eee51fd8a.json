this.selectedTemplate = null\n      this.maintenance = { name: '', type: '', date: '', km: '', details: '', intervalo: '', email: '', alerta: '', fechaUltima: '' }\n    },\n    saveMaintenance() {\n      console.log('[ModalAgregar] saveMaintenance ‚Äî current maintenance:', this.maintenance)\n      if (!this.maintenance) return;\n\n      const nuevaMantencion = {\n        nombre: this.maintenance.name,\n        tipo: this.maintenance.type,\n        intervalo: Number(this.maintenance.intervalo || 0),\n        ultimo: this.selectedTemplate?.type === 'Por Od√≥metro'\n          ? Number(this.maintenance.km || 0)\n          : this.maintenance.fechaUltima,\n        actual: this.selectedTemplate?.type === 'Por Od√≥metro'\n          ? Number(this.maintenance.km || 0)\n          : new Date().toISOString().split('T')[0],\n        alerta: Number(this.maintenance.alerta || 0),\n        email: this.maintenance.email || '',\n        fechaCreacion: new Date().toISOString()\n      };\n\n      console.log('[ModalAgregar] Emitting save with:', nuevaMantencion)\n      this.$emit('save', nuevaMantencion)\n\n      // üîí Marcar este template como usado inmediatamente (esto solo afecta la instancia actual)\n      if (this.selectedTemplate) {\n        const index = this.templates.findIndex(t => t.id === this.selectedTemplate.id);\n        if (index !== -1) {\n          this.templates[index].used = true;\n          console.log('[ModalAgregar] Marked template.used = true for', this.templates[index])\n        }\n      }\n\n      // log para ver el prop mantenciones que el modal recibi√≥ (antes de destruirse)\n      console.log('[ModalAgregar] current prop mantenciones (antes de cerrar):', this.mantenciones)\n      this.cancel()\n    },\n\n    onlyNumber(e) { const char = String.fromCharCode(e.keyCode); if (!/[0-9]/.test(char)) e.preventDefault() }\n  },\n  computed: {\n    ultimoCampoLabel() { if (!this.selectedTemplate) return ''; return this.selectedTemplate.type === 'Por Od√≥metro' ? '√ö